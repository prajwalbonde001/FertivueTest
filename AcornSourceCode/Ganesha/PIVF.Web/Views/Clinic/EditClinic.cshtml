
@{
    Layout = null;
}
<form name="frmEditClinic" role="form" novalidate ng-controller="ClinicController">
    <div class="form_Content form-horizontal" ng-init="IsFromLandingPg=false;show=true">
        <!-- Form Content Start -->
        <div class="row" ng-init="GetSpecData(IsFromLandingPg);">
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for=""><span class="text-Red">*</span>@PIVF.LanguageResource.Resources.lblCode @* @PIVF.Gemino.LanguageResource.Resources.Code *@ </label>@*Code  ::Modified by swatih for localization 27/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" name="txtCode" class="form-control" ng-model="Clinic.Code" id="txtCode" maxlength="10" ng-required="true" tabindex="1">
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for=""><span class="text-Red">*</span>@PIVF.LanguageResource.Resources.lblClinicName @* @PIVF.Gemino.LanguageResource.Resources.Description *@ </label> @*Clinic Name::Modified by swtih for localization 27/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" name="txtDescription" class="form-control" ng-model="Clinic.Description" id="txtDescription" ng-required="true" tabindex="2"  maxlength="100"/>    
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for=""><span class="text-Red">*</span>@PIVF.LanguageResource.Resources.lblClinicContactNo. @* @PIVF.Gemino.LanguageResource.Resources.lblClinicContactNo *@ </label>@*Clinic Contact No ::Modified by swtih for localization 27/7/2020*@
                    <div class="col-sm-3 col-md-3 col-lg-3  p-r-0"> @*ng-class="{'Invalid':frmComp.MobCountryCode.$invalid && frmComp.MobCountryCode.$dirty}"*@
                        <input type="text" class="form-control alignRight" ng-model="Clinic.ResiSTDCode" limit-to="7" maxlength="7" ng-required="true" replace="[^0-9]" with="" id="txtResiSTDCode" name="txtResiSTDCode" placeholder="xxx" tabindex="3">                     
                    </div>
                    <div class="col-sm-9 col-md-9 col-lg-9"> @*ng-class="{'Invalid':frmComp.txtMobNo.$invalid && frmComp.txtMobNo.$dirty}"*@
                        <input type="text" class="form-control alignRight" id="txtContactNo" name="txtContactNo" ng-required="true" multi-validation="NumericArrowBkSpDelSpDCFSlash" maxlength="10" ng-model="Clinic.ContactNo" placeholder="xxx xxx xxxx" tabindex="4">                        
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group MobileContainer">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for=""><span class="text-Red">*</span>@PIVF.LanguageResource.Resources.lblMobileNo. @* @PIVF.Gemino.LanguageResource.Resources.lblMobNo *@ </label>@*Mobile no :: Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-3 col-md-3 col-lg-3 p-r-0" > @*ng-class="{'Invalid':frmComp.MobCountryCode.$invalid && frmComp.MobCountryCode.$dirty}"*@
                        <input type="text" class="form-control alignRight" id="txtMobileCountryCode" name="txtMobileCountryCode" placeholder="xxx" replace="[^a-zA-Z0-9]" with="" ng-model="selectedCountry1" uib-typeahead="i as (i.CountryName) for i in CountryList1 | filter:$viewValue | limitTo:20" typeahead-min-length="1" typeahead-on-select="SelectedCountry1(selectedCountry1)" ng-change="GetCountryCode1()" ng-required="true" tabindex="5">                        
                    </div>
                    <div class="col-sm-9 col-md-9 col-lg-9"> @*ng-class="{'Invalid':frmComp.txtMobNo.$invalid && frmComp.txtMobNo.$dirty}"*@
                        <input type="text" class="form-control alignRight" id="txtMobileNO" name="txtMobileNO" ng-required="true"  ng-pattern="/^\d{1,15}$/" maxlength="15" multi-validation="NumericArrowBkSpDelSpDCFSlash" ng-model="Clinic.MobileNO" placeholder="xxx xxx xxxx" tabindex="6"> @*multi-validation="MobNoStartWith"*@   
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblEmailId @* @PIVF.Gemino.LanguageResource.Resources.lblEmailId *@ </label>@*Email id :: Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" class="form-control" name="txtEmail" ng-model="Clinic.Email" id="txtEmail" tabindex="7" >
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblAddressLine1 @* @PIVF.Gemino.LanguageResource.Resources.lblAddressLine1 *@ </label>@*Address Line 1 :: Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" class="form-control" name="txtAddressLine1" ng-model="Clinic.AddressLine1" id="txtAddressLine1" tabindex="8">
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblAddressLine2 @* @PIVF.Gemino.LanguageResource.Resources.lblAddressLine2 *@ </label>@*Address Line 2 :: Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" class="form-control" name="txtAddressLine2" ng-model="Clinic.AddressLine2" id="txtAddressLine2" tabindex="9">
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblAddressLine3 @* @PIVF.Gemino.LanguageResource.Resources.lblAddressLine3 *@ </label>@*Address Line 3 :: Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" class="form-control" name="txtAddressLine3" ng-model="Clinic.AddressLine3" id="txtAddressLine3" tabindex="10">
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblCountry @* @PIVF.Gemino.LanguageResource.Resources.lblCountry *@ </label>@*Country :: Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <select class="form-control" id="GenderId" ng-model="Clinic.CountryID" ng-options="item.CountryID as item.CountryName for item in CountryListForDD" ng-change="GetStateList1('',Clinic.CountryID);" tabindex="11" ></select>
                        @*<input type="text" name="" class="form-control">*@
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblState @* @PIVF.Gemino.LanguageResource.Resources.lblState *@ </label>@*state :: Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <select id="" ng-model="Clinic.StateID" class="form-control"
                                ng-change="GetCityList1('',Clinic.StateID)"
                                ng-options="item.StateID as item.StateName for item in StateList1" tabindex="12"></select>
                        @*<input type="text" name="" class="form-control">*@
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblCity @* @PIVF.Gemino.LanguageResource.Resources.lblCity *@ </label>@*City :: Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <select id="" ng-model="Clinic.CityID" class="form-control"
                                ng-options="item.CityID as item.CityName for item in CityList1" tabindex="13"></select>
                        @*<input type="text" name="" class="form-control">*@
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblArea @* @PIVF.Gemino.LanguageResource.Resources.lblArea *@ </label>@*Area :: Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" class="form-control"  name="txtArea" ng-model="Clinic.Area" id="txtArea" tabindex="14">
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblPincode @* @PIVF.Gemino.LanguageResource.Resources.lblPincode *@ </label>@*Pincode :: Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" class="form-control" name="txtPinCode" ng-model="Clinic.PinCode" id="txtPinCode"  tabindex="15" maxlength="7">
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblDatabase @* @PIVF.Gemino.LanguageResource.Resources.lblDatabase *@ </label>@*Database :: Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" class="form-control" name="txtDatabaseName" ng-model="Clinic.DatabaseName" id="txtDatabaseName" tabindex="16">
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3" ng-hide="true">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblCluster @* @PIVF.Gemino.LanguageResource.Resources.lblCluster *@ </label>@*Cluster :: Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" class="form-control" name="txtClusterID" ng-model="Clinic.ClusterID" id="txtClusterID" tabindex="17">
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblGSTINNo. @* @PIVF.Gemino.LanguageResource.Resources.lblGSTINNo *@ </label>@*GSTIN No::Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" class="form-control" name="txtGSTNNo" ng-model="Clinic.GSTNNo" id="txtGSTNNo" tabindex="18">
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblPharmacyLicenseNo. @* @PIVF.Gemino.LanguageResource.Resources.lblPharmacyLicenseNo *@ </label>@*Pharmacy License No. ::Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" class="form-control" name="txtPharmacyLicenseNo" ng-model="Clinic.PharmacyLicenseNo" id="txtPharmacyLicenseNo" tabindex="19">
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblClinicFaxNo. @* @PIVF.Gemino.LanguageResource.Resources.lblClinicFaxNo *@ </label>@*Clinic Fax No. ::Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" class="form-control" name="txtFaxNo" ng-model="Clinic.FaxNo" id="txtFaxNo" tabindex="20">
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblCINClinicRegistrationNo. @* @PIVF.Gemino.LanguageResource.Resources.lblClinicRegistrationNo *@ </label>@*(CIN) Clinic Regisrtation No::Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" name="txtCINNo" id="txtCINNo" class="form-control" ng-model="Clinic.CINNo" tabindex="21">
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblPanNo. @* @PIVF.Gemino.LanguageResource.Resources.lblPanNo *@ </label>@*Pan No :: Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" name="txtPanNo" id="txtPanNo" class="form-control" ng-model="Clinic.PanNo" tabindex="22">
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3" ng-hide="true">
                <div class="form-group">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblClinicType @* @PIVF.Gemino.LanguageResource.Resources.lblClinicType *@ </label>@*Clinic Type :: Modified by swatih for localization 28/7/2020*@
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <input type="text" name="" class="form-control">
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12 text-right m-t-10 m-b-10">
                <button class="btn btn-primary" type="button" id="btnModify" ng-click="ModifyClinic(Clinic)" tabindex="23">@PIVF.LanguageResource.Resources.btnUpdate @* @PIVF.Gemino.LanguageResource.Resources.btnUpdate *@ </button>@*Update :: Modified by swatih for localization 28/7/2020*@
                <button class="btn btn-default" type="button" id="" ng-click="Cancel()" tabindex="24">@PIVF.LanguageResource.Resources.btnCancel @* @PIVF.Gemino.LanguageResource.Resources.btnCancel *@ </button>@*Cancel :: Modified by swatih for localization 28/7/2020*@
            </div>
        </div>
    </div>

    <div class="form_Content form-horizontal">
        <div class="row">
            <div class="col-md-12 m-t-20">
                <div class="table-responsive table-bordered" id="" style="max-height:310px; min-height:310px;">
                    <table class="table table-hover GridTable">
                        <thead style="background: #F6F9FB;color: #5895A6;">
                            <tr>
                                <th>@PIVF.LanguageResource.Resources.lblSelect @* @PIVF.Gemino.LanguageResource.Resources.lblSelect *@ </th>@*Select ::Modified by swatih for localization 28/7/2020*@
                                <th>@PIVF.LanguageResource.Resources.lblDepartment @* @PIVF.Gemino.LanguageResource.Resources.lblDepartment *@ </th>@*Department ::Modified by swatih for localization 28/7/2020*@
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="item in DepartmentList">
                                <td><input type="checkbox" ng-model="item.IsActive" /></td>
                                <td>{{item.Description}}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</form>
