                                <span class="MRN-Green m-b-5">
                                    <span>{{registrationNumber }}</span>
                                </span>
                                <span class="RightMobNo LinkName">
                                    <i class="fa fa-volume-control-phone" aria-hidden="true"></i>
                                    <span class="">{{'+'+mobCountryId+' '+mobileNo}}</span>
                                </span>

