@{
    Layout = null;
}
<style>
    .effectfront {
        border: 1px solid #777777;
        margin: 0 auto;
        width: 40px;
        height: 35px;
    }

        .effectfront:hover {
            -webkit-transform: scale(5);
            -moz-transform: scale(5);
            -o-transform: scale(5);
            transform: scale(5);
            transition: all 0.3s;
            -webkit-transition: all 0.3s;
            z-index: 66666;
            position: absolute;
            background: #c0c0c0;
        }

    .valid {
        border: 1px solid none;
        color: #635f5f;
    }

    .invalid {
        box-shadow: none;
        border: 1px solid red;
    }

    .has-error input {
        border: 1px solid red !important;
    }

    .row.display-flex {
        display: flex;
        flex-wrap: wrap;
    }

        .row.display-flex > [class*='col-'] {
            display: flex;
            flex-direction: column;
        }

    /*---------------------------------------*/

    .femaleHistoryTab {
        padding: 20px;
        border: 1px solid #EEEEF0;
        border-radius: 16px;
        background: #FFFFFF;
    }



    .verticalTab .tab-content {
        border: none;
        border-bottom: 1px solid #E2E3E5;
    }

    .femaleHistoryTab .tab-content .tab-pane {
        background: rgba(255, 255, 255, 0.5);
        border: none;
        padding: 0px 10px 10px 0px;
        overflow-y: auto;
        overflow-x: hidden;
        /*height: calc(100vh - 470px) !important;*/
        height: calc(100vh - 300px) !important;
    }


    .femaleHistoryTab .nav-tabs {
        /*height: 665px;
    min-height : 665px;*/
        overflow: auto;
        background: #FAFAFA;
        padding: 20px;
        border-radius: 20px;
        border: none;
        display: flex;
        flex-direction: column;
        gap: 15px;
        /*height: calc(100vh - 415px) !important;*/
        height: calc(100vh - 245px) !important;
    }





        .femaleHistoryTab .nav-tabs > li.active > a:after {
            display: none;
        }


        .femaleHistoryTab .nav-tabs > li.active > a {
            color: #ffffff !important;
            border: 1px solid #d06111 !important;
            background: #df7d36;
            box-shadow: 4px 4px 10px 0px #00000026;
            border: 1px solid #5895A6 !important;
            background: #ECF7F7;
            border-radius: 8px;
            color: #232323 !important;
            font-weight: bold;
        }




        .femaleHistoryTab .nav-tabs li a {
            border-radius: 8px;
            color: #232323;
            font-size: 13px;
            cursor: pointer;
            background: #F4F4F4;
            border: none !important;
        }



        .femaleHistoryTab .nav-tabs > li.active > a:hover, .nav-tabs > li.active > a:focus, .nav-tabs li a, .nav-tabs > li > a:hover {
            font-weight: normal;
            background: #DEEAED;
            color: #232323 !important;
        }
</style>
