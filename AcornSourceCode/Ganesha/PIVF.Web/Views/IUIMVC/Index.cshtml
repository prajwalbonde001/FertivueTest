
@{
    Layout = null;

}
@*Visit Select Model box*@

<script type="text/ng-template" data-backdrop="false" data-keyboard="false" id="visitmodel">
    <table class="table table-hover table-striped table-hover selectPatient_item" id="">
        <thead>
            <tr>
                <th></th>
                <th>@PIVF.LanguageResource.Resources.lblDate</th>
                <th>@PIVF.LanguageResource.Resources.lblOPDNo </th>
                <th>@PIVF.LanguageResource.Resources.tlblVisitID</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="Item in visitInformation">
                <td>
                    <label class="radio-inline">
                        <input type="radio" name="inlineRadioOptions" id="" value="" ng-click="SelectPatient(Item)"> &nbsp;
                    </label>
                </td>
                <td>{{Item.Date | date:"dd-MMM-yyyy"}}</td>
                <td>{{Item.OPDNO}}</td>
                <td>{{Item.VisitID}}</td>
            </tr>
        </tbody>
    </table>
</script>
<form name="frmIUI" novalidate ng-controller="IUICtr" ng-init="Load()" style="height: calc(100vh - 252px);
    overflow: auto;
    overflow-x: hidden;">
    <!-- cycles links -->
    <div class="cycleLinks" style="position: relative;
        z-index: 11;">
        <ul>
            <li ng-click="Navigate('CycleOverview')">@PIVF.LanguageResource.Resources.tlblOverview</li>
            <li ng-click="Navigate('StimulationChart')">@PIVF.LanguageResource.Resources.lblStimulationChart</li>
            <li ng-click="Navigate('SemenDetails')" ng-show="configData.SemenDetail">@PIVF.LanguageResource.Resources.tlblSemenDetails</li>
            <li class="active" ng-click="Navigate('IUI')" ng-show="configData.IUI">@PIVF.LanguageResource.Resources.tlblIUI</li>
            <li ng-click="Navigate('OPU')" ng-show="configData.OPU">@PIVF.LanguageResource.Resources.tlblOPU</li>
            <li ng-click="Navigate('Embrology')" ng-show="configData.Embryology">@PIVF.LanguageResource.Resources.tlblEmbryology</li>
            <li ng-click="Navigate('EmbryoTransfer')" ng-show="configData.EmbryoTransfer">@PIVF.LanguageResource.Resources.tlblEmbryoTransfer</li>
            <li ng-show="configData.CryoPreservation">
                <span class="menu-title">
                    @PIVF.LanguageResource.Resources.lblCryobank
                    <span class="arrow"></span> <!-- Arrow added -->
                </span>
                <ul>
                    <li ng-click="Navigate('OocyteVitrification')" ng-show="configData.OocyteVitrification">@PIVF.LanguageResource.Resources.tlblOocyteVitrification</li>
                    <li ng-click="Navigate('OocyteThowing')" ng-show="configData.OocyteThawing">@PIVF.LanguageResource.Resources.tlblOocyteThawing</li>
                    <li ng-click="Navigate('EmbryoVitrification')" ng-show="configData.EmbryoVitrification">@PIVF.LanguageResource.Resources.tlblEmbryoVitrification</li>
                    <li ng-click="Navigate('EmbryoThowing')" ng-show="configData.EmbryoThawing">@PIVF.LanguageResource.Resources.tlblEmbryoThawing</li>
                </ul>
            </li>
            <li ng-click="Navigate('Outcome')" ng-show="configData.Outcome">@PIVF.LanguageResource.Resources.lblOutcome</li>
        </ul>
    </div>
    <!-- /cycles links -->
    <div class="div60"></div>
    <section class="">
        <div class="form_Content m-t-7" style="position: relative;
        z-index: 11;">
            <!-- Preliminary Details Start -->
            <div class="row form-horizontal">
                <section class="col-lg-12"><h5 class="subTitle">@PIVF.LanguageResource.Resources.tlblPreliminaryDetails</h5></section>
                <div class="col-md-3 col-lg-3">
                    <div class="form-group ">
                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for=""><span class="text-Red">*</span>@PIVF.LanguageResource.Resources.lblDate</label>
                        <div class="col-sm-12 col-md-12 col-lg-12">
                            <div class="input-group date" data-provide="datepicker" ng-class="{'Invalid':frmIUI.PrepDate.$invalid && frmIUI.PrepDate.$dirty }">
                                <input type="text" class="form-control" id="PrepDate" name="PrepDate" placeholder="@PIVF.LanguageResource.Resources.tlblddMmmyyyy" ng-required="true" ng-model="IUI.SprermFreezingDate" aria-describedby="" uib-datepicker-popup="{{format}}" show-button-bar="false" is-open="popup1.opened" datepicker-options="dateOptions" alt-input-formats="altInputFormats" ng-change="IUITime()">
                                <span class="input-group-addon" id="" ng-click="open1()"><i class="fa fa-calendar" aria-hidden="true"></i></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 col-lg-3">
                    <div class="form-group ">
                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for=""><span class="text-Red">*</span>@PIVF.LanguageResource.Resources.lblTime</label>
                        <div class="col-sm-12 col-md-12 col-lg-12">
                            <div uib-timepicker ng-model="IUI.SprermFreezingTime" hour-step="1" class="timeCss" minute-step="5" max="maxTime" show-meridian="false" show-spinners="false" show-seconds="false"></div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 col-lg-3" ng-show="ArtSubTypeID == 3">
                    <div class="form-group ">
                        <button class="btn btn-primary fileinput-button" ng-click="LoadDonorData()" data-toggle="modal" data-target="#selectDonor" type="button" id="" value=""><span class="glyphicon glyphicon-paperclip"></span> @PIVF.LanguageResource.Resources.lblLinkDonor</button>
                        {{IUI.DonorCode}}@*{{IUI.MRNo}}*@
                    </div>
                </div>
                <div class="col-md-3 col-lg-3">
                    <div class="form-group ">
                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for=""><span class="text-Red">*</span>@PIVF.LanguageResource.Resources.tlblTypeofSperm</label>
                        <div class="col-sm-12 col-md-12 col-lg-12">
                            <select class="form-control " id="ddlTypeofSperm" name="ddlTypeofSperm" ng-class="{select:frmIUI.ddlTypeofSperm.$dirty && IUI.SpermTypeID==0}" ng-change="ChangeSpermType()" ng-model="IUI.SpermTypeID" ng-disabled="ArtTypeID == 2 && ArtSubTypeID == 6" ng-options="item.ID as item.Description for item in SpermTypeList"></select>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 col-lg-3">
                    <div class="form-group ">
                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.tlblTIMSI</label>
                        <div class="col-sm-12 col-md-12 col-lg-10 p-r-0">
                            <input type="text" ng-model="IUI.TIMSI" maxlength="100" class="form-control" id="" placeholder="">
                        </div>
                        <label class="col-sm-4 col-md-6 col-lg-2 control-label " for="">@PIVF.LanguageResource.Resources.tlblrpm</label>
                    </div>
                </div>
            </div>
            <!-- Preliminary Details End -->
            <!-- Fresh Sample Details Start -->
            <div ng-show="IUI.SpermTypeID == 1">
                <div class="row form-horizontal" ng-init="IUI.SampleCollected=0">
                    <section class="col-lg-12"><h5 class="subTitle">@PIVF.LanguageResource.Resources.tlblFreshSample</h5></section>
