                            <div class="col-sm-12 col-md-12 col-lg-12">
                                <label class="radio-inline">
                                    <input type="radio" ng-disabled="IsDisableFresh" ng-model="IUI.SampleCollected" id="inlineRadio" value=0> @PIVF.LanguageResource.Resources.tlblInhouse
                                </label>
                                <label class="radio-inline">
                                    <input type="radio" ng-disabled="IsDisableFresh" name="inlineRadioOptions" ng-model="IUI.SampleCollected" value=1> @PIVF.LanguageResource.Resources.tlblOutside
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 col-lg-3">
                        <div class="form-group ">
                            <div class="col-md-6 col-sm-6 col-lg-6">
                                <div class="form-group ">
                                    <label class="col-sm-12 col-md-12 col-lg-12 control-label p-r-0" for="">
                                        <span class="text-Red">*</span>@PIVF.LanguageResource.Resources.tlblCollectionDate
                                    </label>
                                    <div class="col-md-12 col-sm-12 col-lg-12">
                                        <div ng-disabled="IsDisableFresh" class="input-group date" ng-class="{'Invalid':frmIUI.CollectionDate.$invalid && frmIUI.CollectionDate.$dirty && IUI.SpermTypeID == 1 }">
                                            <input type="text" ng-disabled="IsDisableFresh" class="form-control" id="CollectionDate" name="CollectionDate" ng-required="true" placeholder="@PIVF.LanguageResource.Resources.tlblddMmmyyyy" ng-model="IUI.CollectionDate" aria-describedby="" uib-datepicker-popup="{{format}}" show-button-bar="false" is-open="popup2.opened" datepicker-options="dateOptions" alt-input-formats="altInputFormats">
                                            <span class="input-group-addon" ng-disabled="IsDisableFresh" ng-click="open2()"><i class="fa fa-calendar" aria-hidden="true"></i></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-lg-6">
                                <div class="form-group ">
                                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for=""><span class="text-Red">*</span>@PIVF.LanguageResource.Resources.lblTime</label>
                                    <div class="col-md-12 col-sm-12 col-lg-12">
                                        <div ng-disabled="IsDisableFresh" ng-class="{'Invalid':frmIUI.CollectionTime.$invalid && frmIUI.CollectionTime.$dirty && IUI.SpermTypeID == 1 }"
