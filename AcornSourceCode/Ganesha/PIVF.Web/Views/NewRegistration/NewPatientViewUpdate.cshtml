                                        <input type="text" maxlength="20" ng-model="BankData.CustName" replace="[^a-zA-Z\s]" with="" ng-required="true" class="form-control" name="">
                                    </div>
                                </div>
                            </div>

                        </div>

                    </div>
                    <div class="modal-footer">
                        <div class="col-sm-12 text-right">
                            <button class="btn btn-primary" ng-click="AddBankDetails(BankData)" type="button" id="">@PIVF.LanguageResource.Resources.btnSave @*Save*@</button>
