                                    <label class="radio-inline">
                                        <input type="radio" name="searchPatient_{{$index}}" ng-model="i.IsPositive" id="tdbNegative_{{$index}}" value="2" ng-change="StatusInUTPChange(i.IsPositive)"> Negative
