                                   datepicker-options="dateOptionsDOB"
                                   alt-input-formats="altInputFormats">
                            <span class="input-group-addon" id="" ng-click="open1()">
                                <i class="fa fa-calendar" aria-hidden="true"></i>
                            </span>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-md-4 col-lg-3 form-horizontal">
                <div class="form-group ">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">
                        @PIVF.LanguageResource.Resources.lblSNoofsacsobser
                    </label>
                    <div class="col-md-12 col-sm-12 col-lg-12">
                        @*<input type="text" class="form-control" id="" placeholder="" ng-model="Outcome.NoOfSacs" ng-keydown="ValidateNoOfSacs($event)" maxlength="1">*@
                        <select ng-model="Outcome.NoOfSacs" ng-disabled="!IsBCHGEntered" class="form-control" name="ddlNoOfSacs" id="ddlNoOfSacs" @*ng-disabled="!IsBCHGEntered"*@ ng-change="AddUSGRow(Outcome.NoOfSacs)" ng-options="item.ID as item.Description for item in noOfsacsList"></select>
                    </div>
                </div>
            </div><div class="clearfix"></div>
            <div class="col-lg-12">
                <div class="table-responsive table-bordered " style="min-height:130px; max-height:150px;">
                    @*fixed_header*@
                    <!-- min-height:355px;  -->
                    <table class="table table-bordered" id="profile_table" style="min-width:1379px;">
                        <thead>
                            <tr>
                                <th width="">&nbsp;</th>
                                <th width="" class="text-center">@PIVF.LanguageResource.Resources.lblSfetalheart</th>
                                <th width="">@PIVF.LanguageResource.Resources.lblOutcome</th>
                                <th width="" class="">@PIVF.LanguageResource.Resources.lblRemarks</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr class="" ng-repeat="j in USGList">
                                <td>{{j.SacTitle}}</td>
                                <td class="text-center">
                                    <input type="checkbox" ng-model="j.IsFetalHeart">
                                </td>
                                <td>
                                    <select class="form-control " name="ddlOutcome" id="ddlOutcome" ng-change="openGeneticAnalysis(j.OutcomeID)" ng-model="j.OutcomeID" ng-options="item.ID as item.Description for item in OutcomeList"></select>
                                </td>
                                <td>
                                    <input type="text" class="form-control" name="txtremark" id="txtremark" ng-model="j.USGRemark" maxlength="100">
                                </td>
                            </tr>
                            @*<tr class="" ng-repeat="j in USGList">
                                    <td>{{j.SacTitle}}</td>
                                    <td class="text-center">
                                        <input type="checkbox" ng-model="j.IsFetalHeart">
                                    </td>
                                    <td>
                                        <select class="form-control " name="ddlOutcome" id="ddlOutcome" ng-model="j.OutcomeID" ng-options="item.ID as item.Description for item in OutcomeList"></select>
                                    </td>
                                    <td>
                                        <select class="form-control" name="ddlDelivery" id="ddlDelivery" ng-model="j.DeliveryID" ng-options="item.ID as item.Description for item in DeliveryList"></select>
                                    </td>
                                    <td>
                                        <label class="radio-inline">
                                            <input type="radio" name="USG_{{$index}}" id="USGYes_{{$index}}" value="1"  ng-checked="true"> Yes
                                        </label>
                                        <label class="radio-inline">
                                            <input type="radio" name="USG_{{$index}}" id="USGNo_{{$index}}" value="2"> No
                                        </label>
                                    </td>
                                    <td>
                                        <input type="text" name="txtremark" id="txtremark" ng-model="j.USGRemark" >
                                    </td>
                                </tr>*@
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="col-md-4 col-lg-3 form-horizontal">
                <div class="form-group ">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">
                        @PIVF.LanguageResource.Resources.lblComplications
                    </label>
                    <div class="col-md-12 col-sm-12 col-lg-12">
                        <textarea type="text" class="form-control" id="" ng-model="Outcome.Complications" maxlength="250"></textarea>
                    </div>
                </div>
            </div>
            <div class="col-md-4 col-lg-3 form-horizontal">
                <div class="form-group ">
                    <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">
                        @PIVF.LanguageResource.Resources.lblRemarks
                    </label>
                    <div class="col-md-12 col-sm-12 col-lg-12">
                        <textarea type="text" class="form-control" id="" ng-model="Outcome.Remark" maxlength="250"></textarea>
                    </div>
                </div>
            </div><div class="clearfix"></div>

            @*Genetic Analysis of foetus*@
            <div class="form-horizontal" ng-init="IsGenetaicAnalysis=true" ng-hide="IsGenetaicAnalysis">
                <section class="col-md-12 col-lg-12"><h5 class="subTitle">@PIVF.LanguageResource.Resources.lblSGeneticanalysisoffoetus</h5></section>
                <div class="col-md-5 col-lg-4 form-horizontal">
                    <div class="form-group ">
                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">
                            @PIVF.LanguageResource.Resources.lblSUploadreport
                        </label>
                        <div class="col-md-12 col-sm-12 col-lg-12">
                            <label id="" class="UserImageUploadContainer img-attach">
                                <span class="btn btn-primary fileinput-button">
                                    @PIVF.LanguageResource.Resources.lblAttach
                                    <input type="file" id="fileInput" ng-model="Outcome.GeneticAnalysisReportImage" name="fileInput" class="upload" onchange="angular.element(this).scope().handleFileSelect(this)" />
                                </span>
                            </label>
                            <span>{{Outcome.GeneticAnalysisReportFileName}}</span>
                            <button class="btn btn_view" type="button" id="" value="" ng-click="ShowImage()">
                                <span class="glyphicon glyphicon-eye-open"></span>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
            @*Birth Dtls Row*@
            <section class="col-md-12 col-lg-12" data-toggle="collapse" data-target=".collapseBirthDtls" ng-show="IsOutComeSaved && IsBirthDetailsHide"><h5 class="subTitle "><span class="cursorPointer link">@PIVF.LanguageResource.Resources.lblSBirthDetails</span></h5></section>
            <div class="col-md-12 col-lg-12 form-horizontal collapse collapseBirthDtls">
                <div class="row">
                    <div class="col-md-4 col-lg-3 form-horizontal">
                        <div class="form-group ">
                            <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">
                                @PIVF.LanguageResource.Resources.lblSpregnancy
                            </label>
                            <div class="col-md-12 col-sm-12 col-lg-12">
                                <select class="form-control" name="ddlDelivery" id="ddlDelivery" ng-change="updateWeaksOfgestation()" ng-disabled="!IsOutComeSaved" ng-model="Outcome.DeliveryID" ng-options="item.ID as item.Description for item in DeliveryList"></select>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4 col-lg-3 form-horizontal">
                        <div class="form-group ">
                            <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">
                                @PIVF.LanguageResource.Resources.lblSWeeksofgestation
                            </label>
                            <div class="col-md-12 col-sm-12 col-lg-12">
                                <select class="form-control" name="ddlDelivery" id="ddlDelivery" ng-disabled="!IsOutComeSaved" ng-model="Outcome.WeaksOfgestation" ng-options="item.ID as item.Description for item in WeeksList"></select>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4 col-lg-3 form-horizontal">
                        <div class="form-group ">
                            <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">
                                @PIVF.LanguageResource.Resources.lblSTypeofDelivery
                            </label>
                            <div class="col-md-12 col-sm-12 col-lg-12">
                                <select class="form-control" name="ddlDeliveryType" id="ddlDeliveryType" ng-disabled="!IsOutComeSaved" ng-model="Outcome.DeliveryTypeID" ng-options="item.ID as item.Description for item in DeliveryTypeList"></select>

                            </div>
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class="table-responsive table-bordered" style="min-height:100px; max-height:200px;">
                            @*ng-scrollbars ng-scrollbars-config="config" style="min-height:200px; max-height:200px;">*@
                            <!-- min-height:355px;  -->
                            <table class="table table-bordered" id="" style="min-width:1200px;min-height:100px; max-height:200px;">
                                <thead>
                                    <tr>
                                        <th width="">&nbsp;</th>
                                        <th width="">@PIVF.LanguageResource.Resources.lblSBirthweight <br /> @PIVF.LanguageResource.Resources.lblSInpound</th>
                                        <th width="" class="text-center">@PIVF.LanguageResource.Resources.lblSCongentialAbnormality</th>
                                        <th width="" class="">@PIVF.LanguageResource.Resources.lblRemarks</th>
                                        <th width="" class="">@PIVF.LanguageResource.Resources.lblSactivity <br /> @PIVF.LanguageResource.Resources.lblSmuscletone</th>
                                        <th width="" class="">@PIVF.LanguageResource.Resources.lblSPulse</th>
                                        <th width="" class="">@PIVF.LanguageResource.Resources.lblSgrimace <br />@PIVF.LanguageResource.Resources.lblSreflexirritablity</th>
                                        <th width="" class="">@PIVF.LanguageResource.Resources.lblSAppereance <br /> @PIVF.LanguageResource.Resources.lblSSkincolor</th>
                                        <th width="" class="">@PIVF.LanguageResource.Resources.lblSrespiration</th>
                                        <th width="" class="">@PIVF.LanguageResource.Resources.lblSApgarscore</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr class="" ng-repeat="k in BirthdetailList">
                                        <td>{{k.Child}}</td>
                                        <td class="">
                                            <input type="text" class="form-control" id="" ng-model="k.BirthWeight" ng-keypress="valiadeBirthWeight(k.BirthWeight,$event)" ng-class="{'select':k.weightInvalid}">
                                        </td>
                                        <td>
                                            <label class="radio-inline">
                                                <input type="radio" name="USG_{{$index}}" id="USGYes_{{$index}}" ng-model="k.IsCongiAbnorm" value="1" ng-checked="true"> Yes
                                            </label>
                                            <label class="radio-inline">
                                                <input type="radio" name="USG_{{$index}}" id="USGNo_{{$index}}" ng-model="k.IsCongiAbnorm" value="2"> No
                                            </label>
                                        </td>
                                        <td>
                                            <input type="text" class="form-control" id="" ng-model="k.Remark" maxlength="100">
                                        </td>
                                        <td>
                                            <select class="form-control" name="ddlBirthActivity" id="ddlBirthActivity" ng-model="k.ActivityID" ng-change="CalcScore(k,$index)" ng-options="item.ID as item.Description for item in BirthActivityList"></select>
                                        </td>
                                        <td>
                                            <select class="form-control" name="ddlPusle" id="ddlPusle" ng-model="k.PulseID" ng-change="CalcScore(k,$index)" ng-options="item.ID as item.Description for item in PusleList"></select>
                                        </td>
                                        <td>
                                            <select class="form-control" name="ddlGrimace" id="ddlGrimace" ng-model="k.GrimaceID" ng-change="CalcScore(k,$index)" ng-options="item.ID as item.Description for item in GrimaceList"></select>
                                        </td>
                                        <td>
                                            <select class="form-control" name="ddSkinColor" id="ddlSkinColor" ng-model="k.AppearanceID" ng-change="CalcScore(k,$index)" ng-options="item.ID as item.Description for item in SkinColorList"></select>
                                        </td>
                                        <td>
                                            <select class="form-control" name="ddlRespiration" id="ddlRespiration" ng-model="k.RespirationID" ng-change="CalcScore(k,$index)" ng-options="item.ID as item.Description for item in RespirationList"></select>
                                        </td>
                                        <td>{{k.Score}}</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            @*Birth Dtls Row*@
            <div class="col-md-offset-9 col-md-3 col-lg-offset-9 col-lg-3 form-horizontal" id="text-right">
                @*<div class="col-md-12 col-lg-12 form-horizontal">*@
                <div class="form-group">
                    <div class="col-md-3 col-sm-3 col-lg-12" style="padding-top: 60px;">
                        <div ng-show="isSurrogate" style="display:inline-block;font-weight:bold;"><a ng-click="UnLinkSurrogate()" style="text-decoration:underline;">lblSUnlinksurrogate</a></div>
                        <label class="control-label text-Red " id="text-right">
                            @*class="col-md-12 col-sm-12 col-lg-12"*@
                            <input type="checkbox" ng-model="Outcome.IsCycleClose"> @PIVF.LanguageResource.Resources.lblSClosecycle
                        </label>
                    </div>
                </div>
            </div>
        </div>

        <!-- Save Cancle Buttons -->
        <div class="row form-horizontal m-t-10" style="position: relative;
        z-index: 11;">
            <div class="col-lg-12 text-right">
                <!-- <label class="checkbox-inline p-t-0"><input type="checkbox" id="inlineCheckbox1" value="option1"> Finalize</label> -->
                <button class="btn btn-primary" type="button" id="btnSave" value="" ng-disabled="disableSaveBtn" ng-click="SaveOutcome(Outcome)">@PIVF.LanguageResource.Resources.lblSSave</button>
                <button class="btn btn-default" type="button" id="" value="" ng-click="cancel()">@PIVF.LanguageResource.Resources.btnCancel</button>
            </div>
        </div>

        <div class="modal fade edit-modal" id="myModal" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" data-keyboard="false" data-backdrop="static">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" id="btnClosePop" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    </div>
                    <div class="modal-body">
                        <div class="row form-horizontal">
                            <div class="col-md-12 col-lg-12">
                                <div class="text-center" ng-if="extn == 'image'">
                                    <img ng-src="{{GetImage}}" style="width: 85%; height: 61%;" />
                                </div>
                                <div class="wrapper" ng-if="extn != 'image'">
                                    <object ng-show="content" data="{{content}}" type="application/pdf" style="width: 100%; height: 400px;"></object>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <div class="row form-horizontal">
                            <div class="col-lg-12 text-right">
                                <button class="btn btn-primary" type="button" id="" value="" ng-show="extn == 'image'" ng-click="SaveReport()">@PIVF.LanguageResource.Resources.btnDownload</button>
                                <button class="btn btn-primary" type="button" id="" value="" ng-hide="true">@PIVF.LanguageResource.Resources.btnPrint</button> @*ng-show="extn == 'image'"*@
                                <button class="btn btn-default" type="button" id="" value="" ng-click="Close()">@PIVF.LanguageResource.Resources.btnClose</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Outcome model -->
        <div class="modal fade" id="OrderInvestigation" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title" id="myModalLabel">@PIVF.LanguageResource.Resources.lblInvestigations</h4>
                    </div>
                    <div class="modal-body">
                        <!-- Preliminary Details Start -->
                        <div class="row">
                            <div class="col-md-12 col-lg-12">
                                <div class="gridBorder">
                                    <div class="row">
                                        <!-- <section class="col-md-12 col-lg-12"><span class="title">Investigations</span></section> -->
                                        <div class="col-md-12 col-lg-12 form-horizontal">
                                            <div class="form-group ">
                                                <div class="col-md-4 col-lg-4">
                                                    <label class="radio-inline">
                                                        <input type="radio" name="inlineRadioOptions" id="" value=""> @PIVF.LanguageResource.Resources.lblLaboratory
                                                    </label>
                                                    <label class="radio-inline p-l-35">
                                                        <input type="radio" name="inlineRadioOptions" id="" value=""> @PIVF.LanguageResource.Resources.lblRadiology
                                                    </label>
                                                </div>
                                                <div class="col-md-2 col-lg-2 p-l-0 m-t-5">
