                            <div class="InputBoxDiv">
                                <!--ng-disabled="!(Que.Date || Que.DeptID!=0 || Que.PatientName || Que.DocID!=0) ? true :false"-->
                                <button id="" type="button" class="btn btn-primary" ng-click="ClearAdvSearch();GetQueueList(Que)" ng-keypress="($event.which === 13)?GetQueueList(Que,false):0">@PIVF.LanguageResource.Resources.btnSearch</button>  @*ClearAdvSearch();*@
                            </div>
                        </div>
                        <div class="clearfix"></div>
                    </div>
                </div>
                <!-- Main Search Box End-->
                <!-- Advance Search Box Start -->
                <div class="MainSearchBoxContainer" ng-show="show==false">
                    <div class="BlackSearchBoxSection">
                        <div class="SearchInputGroupContainer">
                            <div class="row">
                                @*<div class="col-md-3 col-lg-3">
                                        <div class="InputBoxDiv">
                                            <input class="form-control " type="text" id="" ng-model="Que.MRN" placeholder="MRN" ng-keypress="ClearSearch();($event.which === 13)?GetQueueList(Que,false):0">
