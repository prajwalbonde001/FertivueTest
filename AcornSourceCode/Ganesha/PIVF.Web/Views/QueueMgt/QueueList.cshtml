                            <img ng-if="Pat.PhotoString =='' && Pat.GenderId==1" ng-src="../images/Male_default.jpg" class="profile">
                            <img ng-if="Pat.PhotoString =='' && Pat.GenderId==2" ng-src="../images/Female_default.jpg" class="profile">
                        </div>
                        <div class="ProfileInfoWrap">
                            <p class="m-b-0">
                                <span class="PatientName p-l-20">{{Pat.Prefix}}</span>
                                <span class="PatientName">{{Pat.PatientName}}</span>
                            </p>
                            <ul class="profile-data">
                                <li>
                                    <div class="iconInfoLine">
                                        <i class="fa fa-volume-control-phone" aria-hidden="true"></i>
                                        <!-- <img src="images/Icon_UAppointment.png" alt="Appointment"> -->
                                        <span class="LinkName">{{Pat.MobileCountryCode}} {{Pat.ContactNo1}}</span>
                                    </div>
                                </li>
                                <li>
                                    <div class="iconInfoLine">
                                        <i class="fa fa-volume-control-phone" aria-hidden="true"></i>
                                        @*<span class="LinkName" ng-if="Pat.AltMobNo !=''"> {{Pat.AltMobNoCntryCode}}</span>*@
                                        <span class="LinkName"> {{Pat.ContactNo2}}</span>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row form-horizontal">
                <div class="col-lg-12 col-md-6">
                    <div class="form-group">
                        <label class="col-lg-4 col-md-4 col-sm-4 control-label " for="">@PIVF.LanguageResource.Resources.lblRefDoc :</label>
                        <label class="col-lg-8 col-md-8 col-sm-8 control-label  p-l-0" for="">{{Pat.ReferredDoctor}}</label>
                    </div>
                </div>
                <div class="col-lg-12 col-md-6">
                    <div class="form-group">
                        <label class="col-lg-4 col-md-4 col-sm-4 control-label " for="">@PIVF.LanguageResource.Resources.lblVisitTime :</label>
                        <label class="col-lg-8 col-md-8 col-sm-8 control-label  p-l-0" for="">{{Pat.InTime | date: 'hh:mm a'}}</label>
                    </div>
                </div>
                @*<div class="col-lg-12 col-md-6">
                        <div class="form-group">
                            <label class="col-lg-4 col-md-4 col-sm-4 control-label InfoTitle" for="">@PIVF.LanguageResource.Resources.lblSpecialRegistration :</label>
                            <label class="col-lg-8 col-md-8 col-sm-8 control-label InfoData p-l-0" for="">{{Pat.SpecialReg}}</label>
                        </div>
                    </div>*@

                <div class="col-lg-12 col-md-6">
                    <div class="form-group">
                        <label class="col-lg-4 col-md-4 col-sm-4 control-label " for="">@PIVF.LanguageResource.Resources.lblVisitMarkedby :</label>
                        <label class="col-lg-8 col-md-8 col-sm-8 control-label p-r-0 p-l-0" for="">{{Pat.VisMarkedBy}}</label>
                    </div>
                </div>
            </div>
            <!-- Modal content Patient Info End -->
        </div>
    </div>
    @*</div>
        </div>*@
</script>

