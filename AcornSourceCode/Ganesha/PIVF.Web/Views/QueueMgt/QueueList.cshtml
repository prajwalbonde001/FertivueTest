                                @*height:380px;*@
                                <thead>
                                    <tr>
                                        <th st-ratio="10">&nbsp;</th>
                                        @*<th st-ratio="7">@PIVF.LanguageResource.Resources.lblTokenNo</th>*@
                                        @*<th st-ratio="7">@PIVF.LanguageResource.Resources.lblCabin</th>*@
                                        <th st-ratio="7">@PIVF.LanguageResource.Resources.lblDate</th>
                                        <th st-ratio="9">@PIVF.LanguageResource.Resources.lblOPDNo</th>
                                        <th st-ratio="13">@PIVF.LanguageResource.Resources.lblPatientName</th>
                                        @*<th st-ratio="7">PRN</th>*@
                                        <th st-ratio="14">@PIVF.LanguageResource.Resources.lblSpouseName</th>@*//Modified by SwatiH for Localization on 10/7/2020*@
                                        <th st-ratio="6">@PIVF.LanguageResource.Resources.lblCoupleId</th>@*//Modified by SwatiH for Localization on 10/7/2020*@
                                        <th st-ratio="6">@PIVF.LanguageResource.Resources.lblAgeGender</th>@*//Modified by SwatiH for Localization on 10/7/2020*@
                                        <th st-ratio="13">@PIVF.LanguageResource.Resources.lblDoctor-@PIVF.LanguageResource.Resources.lblDepartment</th>
                                        <th st-ratio="11">@PIVF.LanguageResource.Resources.lblVisitType</th>
                                        @*<th st-ratio="6">@PIVF.LanguageResource.Resources.lblVisitStatus</th>*@
                                        @*<th st-ratio="7">@PIVF.LanguageResource.Resources.lblDepartment</th>*@
                                        @*<th st-ratio="7">@PIVF.LanguageResource.Resources.lblRemark</th>*@
                                        @*<th st-ratio="5">@PIVF.LanguageResource.Resources.lblCloseVisit</th>*@
                                        <th st-ratio="6">&nbsp;</th>
                                        <th st-ratio="6">&nbsp;</th>
                                        <th st-ratio="4">@PIVF.LanguageResource.Resources.lblRemark</th>@*//Modified By SwatiH for Localization on 10/7/2020*@
                                    </tr>
                                </thead>
                                <tbody style="height: calc(100vh - 435px);">
                                    <tr class="parent_row" ng-click="HighliteRow($index)" ng-repeat="item in QueueList" ng-class="{activeTRColor: high==$index}">
                                        <td st-ratio="10">
                                            <a class="link p-r-5" href="" ng-click="MRNoClick(item,'ART')" ng-show="item.CoupleID !== null || (item.PatientCategoryID !== 9 && item.PatientCategoryID !== 10)">@PIVF.LanguageResource.Resources.lblART</a>@*ART ::Modified by swatih for localization 31/7/2020*@
                                            <a class="link p-r-5" href="" ng-click="MRNoClick(item,'EMR')">@PIVF.LanguageResource.Resources.lblEMR</a>@*EMR :: Modified by swatih for localization 28/7/2020*@
                                            <a class="link" href="" ng-click="MRNoClick(item,'Partner')" ng-show="item.PatientCategoryID==14 && LinkConfig.IsLinkPartnerConfig">@PIVF.LanguageResource.Resources.lblLinkPartner</a> @*Link partner ::Modified by swatih for localization 31/7/2020*@
                                            <a class="link" href="" ng-click="MRNoClick(item,'Bill')">@PIVF.LanguageResource.Resources.lblBill</a>    @*Added by Nayan Kamble on 31/12/2019*@@*Bill :: Modified by swatih for localization 28/7/2020*@
                                            <a title="@PIVF.LanguageResource.Resources.lblvVideoConsultation" ng-show="item.VisitTypeID==12"><span class="glyphicon glyphicon-facetime-video"></span></a>
                                        </td>
                                        @*<td st-ratio="7">{{item.TokanNo}}</td>*@
                                        @*<td st-ratio="7">{{item.Cabin}}</td>*@
                                        <td st-ratio="7">{{item.Date | date:'dd-MMM-yyyy'}}<br /><b> {{item.Date | date:'hh:mm'}}</b></td>      @*{{item.InTime | date:'hh:mm'}}*@
                                        <td st-ratio="9">{{item.OPDNO}}</td>
                                        <td st-ratio="13" class="Capitalize">{{item.Prefix}} {{item.PatientName}}<br /><b> {{item.MRNo}} </b> </td>
                                        @*<td st-ratio="7">{{item.MRNo}}</td>*@
                                        <td st-ratio="14" class="Capitalize">{{item.PartnerName}} </td>
                                        <td st-ratio="6">{{item.CoupleID}}</td>
                                        <td st-ratio="6">{{item.Age}} yrs<br /><b>{{item.Gender}}</b></td>
                                        <td st-ratio="13">{{item.DoctorName}}<br /><b>{{item.Department}}</b></td>
                                        <td st-ratio="11">{{item.VisitType}}</td>
                                        @*<td st-ratio="6">{{item.VisitStatus}}</td>*@
                                        @*<td st-ratio="7">{{item.Description}}</td>*@
                                        @*<td st-ratio="7">{{item.Remarks}}</td>*@
                                        @*<td st-ratio="5" class="text-center">
                                                <input type="checkbox" ng-disabled="item.IsVisitClosed1" ng-model="item.IsVisitClosed" ng-click="CloseVisit(item);">
                                            </td>*@
                                        <td st-ratio="6">
                                            <a class="link" href="" ng-click="MRNoClick(item,'donor')" ng-show="item.PatientCategoryID==11 && LinkConfig.IsMarkDonerConfig">@PIVF.LanguageResource.Resources.lblMarkDonor</a>@*Mark Donor :: Modified by swatih for localization 28/7/2020*@
                                        </td>
                                        <td st-ratio="6">
                                            <a class="link" href="" ng-click="MRNoClick(item,'surrogate')" ng-show="item.PatientCategoryID==11 && item.Gender=='Female' && MarkSurrogate=='true' ">@PIVF.LanguageResource.Resources.lblMarkSurrogate</a>    @*&& ClientID==4*@ @*Mark Surrogate :: Modified by swatih for localization 28/7/2020*@
                                        </td>
                                        <td st-ratio="4">
                                            <button ng-attr-type="button" class="btn btn_edit" title="@PIVF.LanguageResource.Resources.lblAddRemark" ng-click="SaveVisitRemark(item,$index)"></button>  @*data-toggle="modal" data-target="#remarkModal"*@
                                            <button ng-attr-type="button" class="btn btn_view clinicList" title="@PIVF.LanguageResource.Resources.lblvViewRemark" data-backdrop="static" data-keyboard="false"><span class="glyphicon glyphicon-eye-open"></span></button>
                                            <div class="hoverabsDiv" style="background:#ffffff">
                                                @*class="hoverabsDiv"*@
                                                {{item.Complaints}}
                                            </div>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>

                    <div class="col-md-12">
                        <uib-pagination total-items="TotalItems" ng-model="CurrentPage" ng-change="PageChange()" max-size="maxSize" class="pagination-sm" boundary-links="true" num-pages="numPages" items-per-page="10" first-text="@PIVF.LanguageResource.Resources.lblFirst" previous-text="@PIVF.LanguageResource.Resources.lblPrevious" next-text="@PIVF.LanguageResource.Resources.lblNext" last-text="@PIVF.LanguageResource.Resources.lblLast"></uib-pagination>
                        <!-- Button group row -->
                    </div>
                </div>
            </div>
        </div>

        <div class="clearfix"></div>
    </div>
    <span us-spinner="{radius:30, width:8, length: 16}" spinner-key="GridSpinner" spinner-start-active="true"></span>
</div>
<div class="clearfix"></div>
<!-- Page Content End -->
<!-- page-wrapper End -->

<script type="text/ng-template" id="AddInfo">
    @*<div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
        <div class="modal-dialog  modal-md">*@
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" ng-click="ReasonCancel();" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title">@PIVF.LanguageResource.Resources.lblAdditionalInformation</h4>
        </div>
        <div class="modal-body" id="ViewQuePatient">
            <!-- Modal content Patient Info Start -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="BlueWrap">
