@{
    Layout = null;
}


<style>
    .SemenAnalysisTabNew {
        padding: 20px;
        border: 1px solid #EEEEF0;
        border-radius: 16px;
        background: #FFFFFF;
        /*height: calc(100vh - 345px);*/
        height: calc(100vh - 170px);
        overflow: auto;
        margin: -10px;
    }



    .verticalTab .tab-content {
        border: none;
        border-bottom: 1px solid #E2E3E5;
        margin-bottom: 0px;
    }



    .SemenAnalysisTabNew .nav-tabs {
        /*height: 665px;
    min-height : 665px;*/
        overflow: auto;
        background: #FAFAFA;
        padding: 20px;
        border-radius: 20px;
        border: none;
        display: flex;
        flex-direction: column;
        gap: 15px;
        height: calc(100vh - 480px) !important;
    }





        .SemenAnalysisTabNew .nav-tabs > li.active > a:after {
            display: none;
        }


        .SemenAnalysisTabNew .nav-tabs > li.active > a {
            color: #ffffff !important;
            border: 1px solid #d06111 !important;
            background: #df7d36;
            box-shadow: 4px 4px 10px 0px #00000026;
            border: 1px solid #5895A6 !important;
            background: #ECF7F7;
            border-radius: 8px;
            color: #232323 !important;
            font-weight: bold;
        }




        .SemenAnalysisTabNew .nav-tabs li a {
            border-radius: 8px;
            color: #232323;
            font-size: 13px;
            cursor: pointer;
            background: #F4F4F4;
            border: none !important;
        }



        .SemenAnalysisTabNew .nav-tabs > li.active > a:hover, .nav-tabs > li.active > a:focus, .nav-tabs li a, .nav-tabs > li > a:hover {
            font-weight: normal;
            background: #DEEAED;
            color: #232323 !important;
        }
</style>














