@{
    Layout = null;
}





<style>
    .semenFreezingTabNew {
        padding: 20px;
        border: 1px solid #EEEEF0;
        border-radius: 16px;
        background: #FFFFFF;
        /*height: calc(100vh - 345px);*/
        height: calc(100vh - 170px);
        overflow: auto;
        margin: -10px;
    }



    .verticalTab .tab-content {
        border: none;
        border-bottom: 1px solid #E2E3E5;
        margin-bottom: 0px;
    }



    .semenFreezingTabNew .nav-tabs {
        /*height: 665px;
    min-height : 665px;*/
        overflow: auto;
        background: #FAFAFA;
        padding: 20px;
        border-radius: 20px;
        border: none;
        display: flex;
        flex-direction: column;
        gap: 15px;
        height: calc(100vh - 480px) !important;
    }





        .semenFreezingTabNew .nav-tabs > li.active > a:after {
            display: none;
        }


        .semenFreezingTabNew .nav-tabs > li.active > a {
            color: #ffffff !important;
            border: 1px solid #d06111 !important;
            background: #df7d36;
            box-shadow: 4px 4px 10px 0px #00000026;
            border: 1px solid #5895A6 !important;
            background: #ECF7F7;
            border-radius: 8px;
            color: #232323 !important;
            font-weight: bold;
        }




        .semenFreezingTabNew .nav-tabs li a {
            border-radius: 8px;
            color: #232323;
            font-size: 13px;
            cursor: pointer;
            background: #F4F4F4;
            border: none !important;
        }



        .semenFreezingTabNew .nav-tabs > li.active > a:hover, .nav-tabs > li.active > a:focus, .nav-tabs li a, .nav-tabs > li > a:hover {
            font-weight: normal;
            background: #DEEAED;
            color: #232323 !important;
        }


    tbody tr {
        background: #FFFFFF !important;
        border-bottom: 1px solid #F4F4F5;
    }


        tbody tr:hover {
            background: #EFF5F7 !important;
        }
</style>






<div class="semenFreezingTab customMainContaint" ng-controller="ctrlSemenFreez" ng-init="LoadData()">
    @*<section class="list_form m-t-35">*@
    <form name="frmSemenFreez" novalidate autocomplete="off">
        @*Vertical Form Start*@
        <div class="row verticalTab  semenFreezingTabNew" id="semenFreezing" style="position: relative;
        z-index: 11;">

            <div class="row">

                <div class="col-xs-4 col-sm-4 col-md-3 col-lg-3 p-r-0">
                    @*<div class="leftTabbg">*@
                    <ul class="nav nav-tabs nav nav-pills nav-stacked">
                        <li class="active"><a data-target="#FreezingDetails" data-toggle="tab"> @PIVF.LanguageResource.Resources.lblcPreFreezingDetails </a></li>
                        <li><a data-target="#MotilityMorphology" data-toggle="tab"> @PIVF.LanguageResource.Resources.lblcMotilityMorphology </a></li>
                        <li><a data-target="#OtherParameters" data-toggle="tab"> @PIVF.LanguageResource.Resources.lblcOtherParameters </a></li>
                        <li><a data-target="#SpermFreezing" data-toggle="tab">  @PIVF.LanguageResource.Resources.lblcCryostoragedetails </a></li>
                    </ul>
                    @*</div>*@
                </div>
                <div class="col-xs-8 col-sm-8 col-md-9 col-lg-9 p-l-0 " style="padding-left:20px !important;">
                    <div class="tab-content">
                        @*Pre Freezing Details Start*@
                        <div class="tab-pane active form-horizontal " id="FreezingDetails">
                            <!-- Preliminary Details Start -->
                            <div class="row form-horizontal">
                                <!-- <section class="col-lg-12"><h5 class="subTitle">Preliminary Details</h5></section> -->
                                <div class="col-md-4 col-lg-3">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblDate</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            <div class="input-group date" ng-class="{'Invalid':frmSemenFreez.FreezDate.$invalid && frmSemenFreez.FreezDate.$dirty }">
                                                <input type="text" class="form-control" id="FreezDate" name="FreezDate" placeholder="dd-Mmm-yyyy" ng-required="true" ng-change="ChangeFreezingTime()" ng-model="SemenFreez.SpremFreezingDate" aria-describedby="" uib-datepicker-popup="{{format}}" show-button-bar="false" is-open="popup1.opened" datepicker-options="dateOptions" alt-input-formats="altInputFormats" tabindex="1">
                                                <span class="input-group-addon" id="" ng-click="open1()"><i class="fa fa-calendar" aria-hidden="true"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-lg-3">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblTime</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12" ng-class="{'Invalid':SemenFreez.SpremFreezingTime=='undefine' || SemenFreez.SpremFreezingTime=='' }">
                                            <div uib-timepicker ng-model="SemenFreez.SpremFreezingTime" class="timeCss" hour-step="1" minute-step="5" max="maxTime" show-meridian="false" show-spinners="false" show-seconds="false" tabindex="2"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- Preliminary Details End -->
                            <!-- Pre Freezing Details Start -->
                            <div class="row form-horizontal">
                                <div class="col-md-12 col-lg-12"><h5 class="subTitle">@PIVF.LanguageResource.Resources.lblcPreFreezingDetails</h5></div>
                                <div class="col-md-6 col-lg-3">
                                    <div class="form-group">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcTypeofSperm</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            <select class="form-control " id="ddlTypeofSperm" name="ddlTypeofSperm" ng-change="EnableDisableLinkBtn(SemenFreez.SpermTypeID)" ng-model="SemenFreez.SpermTypeID" ng-class="{select:SemenFreez.SpermTypeID== 0 && frmSemenFreez.ddlTypeofSperm.$dirty}" ng-required="true" ng-options="item.ID as item.Description for item in SpermTypeList1" tabindex="3"></select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-lg-3">
                                    <div class="form-group p-t-28">
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            <button class="btn btn-primary fileinput-button m-b-0" ng-disabled="IsEnableDisableLinkBtn" ng-click="GetSALinkByPatientID(SemenFreez.SpermTypeID)" data-toggle="modal" data-target="#preFreezing" type="button" id="" value="" tabindex="5">
                                                @*<span class="glyphicon glyphicon-paperclip"></span>*@<span class="glyphicon glyphicon-paperclip"></span> Link
                                            </button>
                                            <button class="btn btn-primary fileinput-button m-b-0" ng-click="RedirectToRetrival(ViewBtnName)" ng-disabled="IsViewBtnDisable" data-toggle="modal" data-target="#SurgicalRetrivalModel" type="button" id="" value="">{{ViewBtnName}}</button>
                                        </div>
                                    </div>
                                </div>
                                <div class="clearfix"></div>
                                <div class="col-md-4 col-lg-3">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcCollectionDates</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            <div class="input-group date" ng-class="{'Invalid':frmSemenFreez.ColleDate.$invalid && frmSemenFreez.ColleDate.$dirty  }">
                                                <input type="text" class="form-control" id="ColleDate" name="ColleDate" ng-required="true" placeholder="dd-Mmm-yyyy" ng-model="SemenFreez.CollectionDate" ng-change="ChangeCollectionTime()" aria-describedby="" uib-datepicker-popup="{{format}}" show-button-bar="false" is-open="popup3.opened" datepicker-options="dateOptions" alt-input-formats="altInputFormats" tabindex="7">
                                                <span class="input-group-addon" id="" ng-click="open3()"><i class="fa fa-calendar" aria-hidden="true"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-lg-3">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcCollectionTimes</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            @*ng-class="{'Invalid':SemenFreez.CollectionTime='undefine' || SemenFreez.CollectionTime=''}"*@
                                            <div uib-timepicker ng-model="SemenFreez.CollectionTime" class="timeCss" hour-step="1" minute-step="1" max="maxTime" show-meridian="false" show-spinners="false" show-seconds="false" tabindex="8"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4 col-lg-3">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcReceivingDates</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            <div class="input-group date" ng-class="{'Invalid':frmSemenFreez.ReceDate.$invalid && frmSemenFreez.ReceDate.$dirty}">
                                                <input type="text" class="form-control" id="ReceDate" name="ReceDate" ng-required="true" ng-change="ChangeReceivingTime()" placeholder="dd-Mmm-yyyy" ng-model="SemenFreez.ReceivingDate" aria-describedby="" uib-datepicker-popup="{{format}}" show-button-bar="false" is-open="popup4.opened" datepicker-options="dateOptions" alt-input-formats="altInputFormats" tabindex="9">
                                                <span class="input-group-addon" id="" ng-click="open4()"><i class="fa fa-calendar" aria-hidden="true"></i></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-lg-3">
                                    <div class="form-group">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcReceivingTimes</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            @*ng-class="{'Invalid':SemenFreez.ReceivingTime='undefine' || SemenFreez.ReceivingTime=''}"*@
                                            <div uib-timepicker ng-model="SemenFreez.ReceivingTime" class="timeCss" hour-step="1" minute-step="1" max="maxTime" show-meridian="false" show-spinners="false" show-seconds="false" tabindex="10"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4 col-lg-3">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcAbstinenceDayss</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            <select class="form-control " id="ddlAbstinence" name="ddlAbstinence" ng-model="SemenFreez.AbstinenceID" ng-options="item.ID as item.Description for item in AbstinenceList" tabindex="11"></select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4 col-lg-3">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcViscosity</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            <select class="form-control " id="ddlViscosity" name="ddlViscosity" ng-model="SemenFreez.ViscosityID" ng-options="item.ID as item.Description for item in ViscosityList" tabindex="12"></select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4 col-lg-3">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcVolume</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12" ng-class="{'Invalid':frmSemenFreez.txtVolume.$invalid && frmSemenFreez.txtVolume.$dirty}">
                                            <input type="text" class="form-control" id="txtVolume" name="txtVolume" ng-required="true" allow-decimal-numbers ng-change="SemenFreez.SpermCount=SemenFreez.SpermConcentration*SemenFreez.Volume" ng-model="SemenFreez.Volume" tabindex="13">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4 col-lg-3">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcSpermConcentrationmillml</label>
                                        <div class="col-md-12 col-sm-12 col-lg-12" ng-class="{'Invalid':frmSemenFreez.txtSpermConce.$invalid && frmSemenFreez.txtSpermConce.$dirty}">
                                            <input type="text" class="form-control" id="txtSpermConce" name="txtSpermConce" ng-change="SemenFreez.SpermCount=SemenFreez.SpermConcentration*SemenFreez.Volume" ng-required="true" replace="[^0-9]" with="" ng-model="SemenFreez.SpermConcentration" maxlength="3" tabindex="14">
                                        </div>
                                        @*<label class="col-md-3 col-sm-2 col-lg-2 lastLabel control-label" for="">Mill/ml</label>*@
                                    </div>
                                </div>
                                <div class="col-md-4 col-lg-3">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcTotalSpermCountmillions</label>
                                        <div class="col-md-12 col-sm-12 col-lg-12">
                                            <input type="text" class="form-control" id="txtSpermCount" name="txtSpermCount" placeholder="" ng-readonly="true" ng-model="SemenFreez.SpermCount" tabindex="15">
                                        </div>
                                        @*<label class="col-md-3 col-sm-2 col-lg-2 lastLabel control-label" for="">millions</label>*@
                                        @*<label class="col-sm-4 col-md-6 col-lg-3 control-label" for="">≥ 39 mill/ejaculate</label>*@<label class="col-sm-4 col-md-6 col-lg-3 control-label" for=""></label><label class="col-sm-4 col-md-6 col-lg-3 control-label" for=""></label>
                                    </div>
                                </div>
                            </div>
                            <!-- Pre Freezing Details End -->
                        </div>
                        @*Pre Freezing Details End*@
                        @*Motility & Morphology Start*@
                        <div class="tab-pane form-horizontal" id="MotilityMorphology">
                            <!-- Motility Assessment Start -->
                            <div class="row form-horizontal">
                                <section class="col-lg-12"><h5 class="subTitle">@PIVF.LanguageResource.Resources.lblcMotilityAssessment</h5></section>
                                <div class="col-md-6 col-lg-6">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcProgressive</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12" ng-class="{'Invalid':frmSemenFreez.txtProgressive.$invalid && frmSemenFreez.txtProgressive.$dirty}">
                                            @*ng-change="SemenFreez.Motility=SemenFreez.GradeA*1 + SemenFreez.GradeB*1"*@
                                            <input type="text" class="form-control" id="txtProgressive" name="txtProgressive" ng-focus="OGradeA = SemenFreez.GradeA" ng-required="true" replace="[^0-9]" with="" maxlength="2" ng-blur="CalculateMotilityAss()" ng-model="SemenFreez.GradeA" tabindex="16">
                                        </div>
                                        @*<label class="col-md-2 col-sm-2 col-lg-2 lastLabel control-label p-l-0" for="">%</label>*@
                                    </div>
                                </div>
                                <div class="col-md-6 col-lg-6">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcNonProgressives</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12" ng-class="{'Invalid':frmSemenFreez.txtNonProgressive.$invalid && frmSemenFreez.txtNonProgressive.$dirty}">
                                            @*ng-change="SemenFreez.Motility=SemenFreez.GradeA*1 + SemenFreez.GradeB*1"*@
                                            <input type="text" class="form-control" id="txtNonProgressive" name="txtNonProgressive" ng-focus="OGradeB = SemenFreez.GradeB" ng-required="true" ng-blur="CalculateMotilityAss()" replace="[^0-9]" with="" maxlength="2" ng-model="SemenFreez.GradeB" tabindex="17">
                                        </div>
                                        @*<label class="col-md-2 col-sm-2 col-lg-2 lastLabel control-label p-l-0" for="">%</label>*@
                                    </div>
                                </div>
                                <div class="col-md-6 col-lg-6">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcImmotiles</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12" ng-class="{'Invalid':frmSemenFreez.txtImmotile.$invalid && frmSemenFreez.txtImmotile.$dirty}">
                                            @*<input type="text" class="form-control" id="txtImmotile" name="txtImmotile" ng-readonly="true" ng-focus="OGradeC = SemenFreez.GradeC" ng-required="true" ng-blur="CalculateMotilityAss()" replace="[^0-9]" with="" maxlength="2" ng-model="SemenFreez.GradeC" tabindex="18">*@
                                            @*Commented & modified on 29Jul2021 to solve validation issue while save*@
                                            <input type="text" class="form-control" id="txtImmotile" name="txtImmotile" ng-readonly="true" ng-focus="OGradeC = SemenFreez.GradeC" ng-required="true" ng-blur="CalculateMotilityAss()" replace="[^0-9]" with="" maxlength="3" ng-model="SemenFreez.GradeC" tabindex="18">
                                        </div>
                                        @*<label class="col-md-2 col-sm-2 col-lg-2 lastLabel control-label p-l-0" for="">%</label>*@
                                    </div>
                                </div>
                                <div class="col-md-6 col-lg-6">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcTotalMotilitys</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            <input type="text" class="form-control" id="txtMotility" name="txtMotility" ng-readonly="true" ng-change="OGradeC = SemenFreez.GradeC" ng-model="SemenFreez.Motility" tabindex="19">
                                        </div>
                                        @*<label class="col-md-2 col-sm-2 col-lg-2 lastLabel control-label p-l-0" for="">%</label>*@
                                    </div>
                                </div>
                            </div>
                            <!-- Motility Assessment Start -->
                            <!-- Advanced Total Progressive Motility Start -->
                            <div class="row form-horizontal" ng-init="IsMandatory=false" ng-hide="1">
                                <section class="col-lg-12"><h5 class="subTitle">@PIVF.LanguageResource.Resources.lblcAdvancedMotility</h5></section>
                                <div class="col-md-6 col-lg-6">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcSlowProgressives</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12" ng-class="{'Invalid':IsMandatory}">
                                            <input type="text" class="form-control" id="txtSlowProgressive" name="txtSlowProgressive" ng-readonly="disable" ng-focus="OSlowProgressive = SemenFreez.SlowProgressive" ng-blur="CalcRapidProg()" multi-validation="NumericArrowBkSpDelSpDCFSlash" maxlength="2" ng-required="IsMandatory" ng-model="SemenFreez.SlowProgressive" tabindex="20">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-lg-6">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcRapidProgressives</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12" ng-class="{'Invalid':IsMandatory}">
                                            <input type="text" class="form-control" id="txtRapidProgressive" name="txtRapidProgressive" ng-readonly="disable" ng-focus="ORapidProgressive = SemenFreez.RapidProgressive" ng-blur="(SemenFreez.RapidProgressive!=undefined ||SemenFreez.RapidProgressive!='')?CalcSlowProg():0" multi-validation="NumericArrowBkSpDelSpDCFSlash" maxlength="2" ng-required="IsMandatory" ng-model="SemenFreez.RapidProgressive" tabindex="21">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- Advanced Total Progressive Motility End -->
                            <!-- Morphology Assessment Start -->
                            <div class="row form-horizontal">
                                <section class="col-lg-12"><h5 class="subTitle">@PIVF.LanguageResource.Resources.lblcMorphologyAssessment</h5></section>
                                <div class="col-md-6 col-lg-6">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcNormalForms</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            <input type="text" class="form-control" id="txtNormalForms" name="txtNormalForms" ng-blur="SemenFreez.AbnormalForms=100 - SemenFreez.NormalForms*1" replace="[^0-9]" with="" maxlength="2" ng-model="SemenFreez.NormalForms" tabindex="22">
                                        </div>
                                        @*<label class="col-md-2 col-sm-2 col-lg-2 lastLabel control-label p-l-0" for="">%</label>*@
                                    </div>
                                </div>
                                <div class="col-md-6 col-lg-6">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcAbnormalForms</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            @*<input type="text" class="form-control" id="txtAbnormalForms" name="txtAbnormalForms" ng-blur="SemenFreez.NormalForms=100 - SemenFreez.AbnormalForms*1" replace="[^0-9]" with="" maxlength="2" ng-model="SemenFreez.AbnormalForms" tabindex="23">*@
                                            @*Commented & modified on 29Jul2021 to solve validation issue while save*@
                                            <input type="text" class="form-control" id="txtAbnormalForms" name="txtAbnormalForms" ng-blur="SemenFreez.NormalForms=100 - SemenFreez.AbnormalForms*1" replace="[^0-9]" with="" maxlength="3" ng-model="SemenFreez.AbnormalForms" tabindex="23">
                                        </div>
                                        @*<label class="col-md-2 col-sm-2 col-lg-2 lastLabel control-label p-l-0" for="">%</label>*@
                                    </div>
                                </div>
                            </div>
                            <!-- Morphology Assessment End -->
                        </div>
                        @*Motility & Morphology End*@
                        @*Other Parameters Start*@
                        <div class="tab-pane form-horizontal" id="OtherParameters">
                            <!-- Start -->
                            <div class="row form-horizontal">
                                <div class="col-md-12 col-lg-12"><h5 class="subTitle">@PIVF.LanguageResource.Resources.lblcOtherParameters</h5></div>
                                <div class="col-md-6 col-lg-6">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcWBC</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            <input type="text" class="form-control" id="txtWBC" name="txtWBC" replace="[^a-zA-Z0-9]" with="" ng-model="SemenFreez.WBC" maxlength="20" tabindex="24">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-lg-6">
                                    <div class="form-group ">
                                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblcPusCells</label>
                                        <div class="col-sm-12 col-md-12 col-lg-12">
                                            <input type="text" class="form-control" id="txtPusCells" name="txtPusCells" replace="[^a-zA-Z0-9]" with="" ng-model="SemenFreez.PusCells" maxlength="20" tabindex="25">
