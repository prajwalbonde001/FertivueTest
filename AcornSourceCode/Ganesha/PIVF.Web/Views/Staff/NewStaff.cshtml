                                                    on-streaming="onSuccess()"
                                                    on-error="onError(err)"
                                                    on-stream="onStream(stream)">
                                            </webcam>
                                            <canvas ng-hide="true" id="snapshot" width="300" height="300"></canvas>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-offset-3 col-sm-6">
                                                <div class="form-group text-center">
                                                    <button type="button" class="btn btn-primary  m-t-10" ng-click="makeSnapshot()"><span><i class="fa fa-camera-retro"></i></span>@PIVF.LanguageResource.Resources.btnCapture</button>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-offset-3 col-sm-6 cropArea  m-b-10">
                                                <ui-cropper image="myImage" live-view="blockingObject" result-image="myCroppedImage" result-image-format="image/jpeg">
                                                </ui-cropper>
                                            </div>
                                        </div>
                                        @*<div class="col-sm-offset-3 col-sm-6">
                                                <div class="form-group text-center">
                                                    <button type="button" class="btn btn-primary"><span><i class="fa fa-camera-retro"></i></span>@PIVF.Gemino.LanguageResource.Resources.btnCapture</button>
                                                </div>
                                            </div>*@<div class="col-sm-offset-3 col-sm-6">
                                            <div class="form-group text-center">
                                                <button type="button" class="btn btn-primary"><span><i class="fa fa-camera-retro"></i></span></button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>                                <!-- page-wrapper Start -->
                            <!-- Page Content Start -->
                            @{
