<div class="" ng-controller="ClinicConfigController" ng-init="LoadData()">
    <div class="form_Content">
        <div class="row m-t-10">
            <div class="col-sm-12 col-md-12 col-lg-12">
                <!-- Patient List Content Container Start-->
                <div class="table-responsive table-bordered fixed_header">
                    <table id="PatientListTble" class="table table-striped table-hover" style="min-height:350px;"> @*max-height:350px;*@
                        <thead>
                            <tr>
                                <th st-ratio="25">@PIVF.LanguageResource.Resources.lblSr_No.</th>@*Sr. No. :: Modified by swatih for localization 24/7/2020*@
                                <th st-ratio="50">@PIVF.LanguageResource.Resources.lblClinic</th> @*Clinic  :: Modified by swatih for localization 24/7/2020*@
                                <th st-ratio="25">@PIVF.LanguageResource.Resources.lblDateValidation</th>@*Date Validation :: Modified by swatih for localization 24/7/2020*@
                            </tr>
                        </thead>
                        <tbody style="height: calc(100vh - 320px);">
                            <tr class="" ng-repeat="item in ClinicList">
                                <td st-ratio="25">{{$index+1}}</td>
                                <td st-ratio="50">{{item.clinicName}}</td>
                                <td class="" st-ratio="25"><input type="checkbox" ng-model="item.IsDateValidation"></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <!--  Patient List Content Container End -->
            </div>
        </div>

        <div class="row">
            <div class="col-lg-12 text-right m-t-10">
                <button class="btn btn-primary" ng-click="Save()" type="button" id="" value="">@PIVF.LanguageResource.Resources.btnSave</button> @*Save ::Modified by swatih  for localization 24/7/2020*@
                @*<button class="btn btn-default" type="button" ng-click="Cancel()">Cancel</button>*@
            </div>
        </div>
    </div>

</div>


