                    </div>
                    <div class="modal-footer">
                        <button ng-attr-type="button" class="btn btn-primary" ng-click="SaveReason()">@PIVF.LanguageResource.Resources.btnSave</button>
                        <button ng-attr-type="button" class="btn btn-default" data-dismiss="modal" aria-hidden="true">@PIVF.LanguageResource.Resources.btnCancel</button>
                    </div>
                </div>
            </div>
        </div>
        <!-- /ART Type model -->
        <!-- Checkbox checked model -->
        <div class="modal fade checkedPopup" id="btnClosePop" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel">
            <div class="modal-dialog modal-sm" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button ng-attr-type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title" id="myModalLabel">@PIVF.LanguageResource.Resources.lblAppointmentReason</h4>
                    </div>
                    <div class="modal-body">
                        <label class="radio-inline" ng-init="InhouseOrOutside='Inhouse'">
                            <input type="radio" name="inlineRadioOptions" id="Inhouse" value="Inhouse" ng-model="InhouseOrOutside" checked> @PIVF.LanguageResource.Resources.lblSInhouse
                        </label>
                        <label class="radio-inline">
                            <input type="radio" name="inlineRadioOptions" id="Outside" value="Outside" ng-model="InhouseOrOutside"> @PIVF.LanguageResource.Resources.lblSOutside
                        </label>
                    </div>
                    <div class="modal-footer">
                        <button ng-attr-type="button" class="btn btn-primary" ng-click="SetDoctorName(InhouseOrOutside)">@PIVF.LanguageResource.Resources.btnSave</button>
                        <button ng-attr-type="button" class="btn btn-default" ng-click="DrugAdministrationCancel()" aria-hidden="true">@PIVF.LanguageResource.Resources.btnCancel</button> @*data-dismiss="modal"*@
                    </div>
                </div>
            </div>
        </div>
        <!-- /Checkbox checked model -->
        <div class="modal fade uiModal" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button ng-attr-type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title" id="myModalLabel">@PIVF.LanguageResource.Resources.lblShormonalgraphs</h4>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div style="width:100%;">
                                <canvas id="myChart"></canvas>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button ng-attr-type="button" class="btn btn-default" data-dismiss="modal" aria-hidden="true">@PIVF.LanguageResource.Resources.btnCancel</button>
                    </div>
                </div>
            </div>
        </div>

    </div>

