
@{
    Layout = null;
}

<form name="frmCoupleBank" novalidate autocomplete="off">
    <div ng-controller="CoupleBankCtr" ng-init="LoadDefault()">
        <div class="form_Content" style="position: relative;
        z-index: 11;">
            <div class="row m-t-10">
                <div class="col-md-4 col-md-offset-4 col-lg-4 col-lg-offset-4 form-horizontal">
                    <div class="form-group">
                        <div class="input-group">
                            <input type="text" class="form-control" id="txtfullName" ng-blur="model=''" placeholder="Name/MRN" name="txtUsrName" ng-model="model" uib-typeahead="i as (i.PatientName) for i in getMatchingPatient($viewValue) | limitTo : 8" typeahead-on-select="onSelect(model)" typeahead-input-formatter="a" autofocus>
                            <span class="input-group-addon glyphicon glyphicon-search btn" type="button"></span>
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-lg-4  text-right">
                    <button class="btn-link"><span class="glyphicon glyphicon-print"></span></button>
                </div>

                <div class="col-sm-12" style="height: calc(100vh - 350px); overflow: auto; margin-top: 10px; margin-bottom: 10px;">

                    <div class="col-md-12 col-lg-12 m-b-5" ng-hide="IsPatientNotExist" style="position: sticky; top: 0; background: #FFFFFF; padding-bottom: 10px; z-index: 1;">
                        <div class="PatientInfoWrapper">
                            <div class="BlueWrap">
                                <div class="ProfileImgWrap">
                                    <img src="~/images/Female_default.jpg" class="profile">
                                </div>
                                <div class="ProfileInfoWrap">
                                    <p class="m-b-0">
                                        <span class="PatientName">@PIVF.LanguageResource.Resources.plblMrss.</span>
                                        <span class="PatientName">{{CoupleDetails.FemalePatient.FemalePatientName}}</span>
                                        <span class="MRN-Green">
                                            <span>{{CoupleDetails.FemalePatient.FemalePatientMRNO}}</span>
                                            <span style="font-size: 20px;color: #FFFFFF;">/</span>

                                        </span>
                                        <span class="PatientName p-l-10">@PIVF.LanguageResource.Resources.plblMr</span>
                                        <span class="PatientName">{{CoupleDetails.MalePatient.MalePatientName}}</span>
                                        <span class="MRN-Green">
                                            <span>{{CoupleDetails.MalePatient.MaleMRNO}}</span>
                                        </span>


                                    </p>
                                    <ul class="profile-data">
                                        <li style="width:63%;">
                                            <span class="patient-gander p-l-0">@PIVF.LanguageResource.Resources.plblCoupleId: {{CoupleRegNo}}</span>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>




                    <h5 class="subTitle" style="margin-bottom:16px">Oocyte Bank</h5>
                    <div style="min-height:150px; margin-bottom:12px; border-radius:10px" class="table-responsive table-bordered fixed_header">
                        <table class="table" style="min-width:2500px;">
                            <thead>
                                <tr>
                                    <th ng-show="!RenewExpiryDatePatient.IsShowDiscard" st-ratio="3">@PIVF.LanguageResource.Resources.lblSelect</th>
                                    <th st-ratio="5" ng-click="SortData('EmbNumber')">@PIVF.LanguageResource.Resources.plblOocyteNo.<div ng-class="sortClass"></div> </th>
                                    <th st-ratio="10" ng-hide="IsSinglePatient" ng-click="SortData('MRNo')">@PIVF.LanguageResource.Resources.lblmrnoo<div ng-class="sortClass"></div></th>
                                    <th st-ratio="10" ng-hide="IsSinglePatient" ng-click="SortData('PatientName')">@PIVF.LanguageResource.Resources.lblPatientName<div ng-class="sortClass"></div></th>
                                    <th st-ratio="8" ng-click="SortData('CycleCode')">@PIVF.LanguageResource.Resources.lblARTCycle<div ng-class="sortClass"></div></th>
                                    <th st-ratio="8" ng-click="SortData('VitrificationNo')">@PIVF.LanguageResource.Resources.plblFreezing<div ng-class="sortClass"></div></th>
                                    <th st-ratio="8" ng-click="SortData('TankDescription')">@PIVF.LanguageResource.Resources.plblFreezingDate / @PIVF.LanguageResource.Resources.plbltime  <div ng-class="sortClass"></div></th>
                                    <th st-ratio="7" ng-click="SortData('ExpiryDate')">@PIVF.LanguageResource.Resources.plblpex / @PIVF.LanguageResource.Resources.plbltime <div ng-class="sortClass"></div></th>
                                    <th st-ratio="5" ng-click="SortData('TankDescription')">@PIVF.LanguageResource.Resources.plblTank<div ng-class="sortClass"></div></th>
                                    <th st-ratio="5" ng-click="SortData('CanistorDescription')">@PIVF.LanguageResource.Resources.lblCanister<div ng-class="sortClass"></div></th>
                                    <th st-ratio="6" ng-click="SortData('GlobletColorDescription')">@PIVF.LanguageResource.Resources.plblGlobletcolor<div ng-class="sortClass"></div></th>
                                    <th st-ratio="6" ng-click="SortData('GlobletColorDescription')" ng-hide="true">@PIVF.LanguageResource.Resources.plblGlobletSize<div ng-class="sortClass"></div></th>
                                    <th st-ratio="5" ng-click="SortData('GlobletSizeDescription')">@PIVF.LanguageResource.Resources.plblcryolock<div ng-class="sortClass"></div></th>
                                    <th st-ratio="6" ng-click="SortData('MediaUsed')">@PIVF.LanguageResource.Resources.plblmediaused<div ng-class="sortClass"></div></th>
                                    <th st-ratio="5" ng-click="SortData('LotNo')">@PIVF.LanguageResource.Resources.plbllot<div ng-class="sortClass"></div></th>
                                    <th st-ratio="7" ng-click="SortData('MediaExpiryDate')">@PIVF.LanguageResource.Resources.plblmediaex<div ng-class="sortClass"></div></th>
                                    <th st-ratio="6" ng-click="SortData('CryolockID')">@PIVF.LanguageResource.Resources.plblMaturity<div ng-class="sortClass"></div></th>
                                    <th st-ratio="4">@PIVF.LanguageResource.Resources.plblhistory</th>
                                    <th st-ratio="4" ng-click="SortData('FormNo')">@PIVF.LanguageResource.Resources.lblStatus<div ng-class="sortClass"></div></th>
                                </tr>
                            </thead>
                            <tbody style="max-height: 250px;">
                                <tr ng-repeat="item in VitriDetails.ListVitriDetails |orderBy:SortColumn:reverseSort" ng-class="{trRed :item.ExpiryDate < TodayDate,trGreen :item.ExpiryDate >TodayDate && item.ExpiryDate <=DateAfter15Days }">
                                    @*,'trGreen' : item.ExpiryDate > TodayDate && item.ExpiryDate <= DateAfter15Days*@
                                    <td ng-show="!RenewExpiryDatePatient.IsShowDiscard" st-ratio="3"><input type="checkbox" ng-click="AddSelectedPatient(item)" ng-model="item.selected"></td>
                                    <td st-ratio="5">{{item.EmbNumber}}</td>
                                    <td st-ratio="10" ng-hide="IsSinglePatient">{{item.MRNo}}</td>
                                    <td st-ratio="10" ng-hide="IsSinglePatient">{{item.PatientName}}</td>
                                    <td st-ratio="8">{{item.CycleCode}}</td>
                                    <td st-ratio="8">{{item.VitrificationNo}}</td>
                                    <td st-ratio="8">{{item.VitriDate | date :  "dd-MMM-yyyy"}}</br>{{item.VitriTime | date : "HH:mm"}}</td>
                                    <td st-ratio="7">{{item.ExpiryDate | date :  "dd-MMM-yyyy"}}</br>{{item.ExpiryTime | date : "HH:mm"}}</td>
                                    <td st-ratio="5">{{item.TankDescription}}</td>
                                    <td st-ratio="5">{{item.CanistorDescription}}</td>
                                    <td st-ratio="6">{{item.GlobletColorDescription}}</td>
                                    <td st-ratio="6" ng-hide="true">{{item.GlobletSizeDescription}}</td>
                                    <td st-ratio="5">{{item.CryoLockDescription}}</td>
                                    <td st-ratio="6">{{item.MediaUsed}}</td>
                                    <td st-ratio="5">{{item.LotNo}}</td>
                                    <td st-ratio="7">{{item.MediaExpiryDate | date :  "dd-MMM-yyyy"}}</td>
                                    <td st-ratio="6">{{item.OocyteMaturityDescription}}</td>
                                    <td st-ratio="4"><button class="btn btn_history" type="button" id="" value="" data-toggle="modal" data-target="#historymodal" ng-click="RedirectToHistoryCryo(item)"></button></td>
                                    <td st-ratio="4">{{item.CustomStatus}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>

                    <div class="row" style="margin-bottom:24px">
                        <div class="col-md-12 col-lg-12">
                            <label class="checkbox-inline p-l-25">
                                <input type="checkbox" id="" ng-model="RenewExpiryDatePatient.IsShowDiscard" ng-change="ShowDiscardChangeOocyte(RenewExpiryDatePatient.IsShowDiscard,CryoBankOocyte.StatusID)" value="option1"> @PIVF.LanguageResource.Resources.plblshowdis
                            </label>
                            <label class="checkbox-inline p-l-25">
                                <span class="trcolorGreen glyphicon glyphicon-stop "></span>@PIVF.LanguageResource.Resources.plblNearingex
                            </label>
                            <label class="checkbox-inline p-l-25">
                                <span class="trcolorRed glyphicon glyphicon-stop "></span>@PIVF.LanguageResource.Resources.plblexpired
                            </label>
                        </div>
                    </div>

                    <h5 class="subTitle" style="margin-bottom:16px">@PIVF.LanguageResource.Resources.plblembryo</h5>
                    <div style="min-height:150px; margin-bottom:12px; border-radius:10px" class="table-responsive table-bordered fixed_header">
                        <table class="table" style="min-width:2500px;">
                            <thead>
                                <tr>
                                    <th st-ratio="3">@PIVF.LanguageResource.Resources.lblSelect</th>
                                    <th st-ratio="6" ng-click="SortData('EmbNumber')">@PIVF.LanguageResource.Resources.plblEmbryoNo. <div ng-class="sortClass"></div></th>
                                    <th st-ratio="6" ng-hide="IsSinglePatient" ng-click="SortData('MRNo')">@PIVF.LanguageResource.Resources.lblmrnoo.<div ng-class="sortClass"></div></th>
                                    <th st-ratio="8" ng-hide="IsSinglePatient" ng-click="SortData('PatientName')">@PIVF.LanguageResource.Resources.lblPatientName<div ng-class="sortClass"></div></th>
                                    <th st-ratio="7" ng-click="SortData('CycleCode')">@PIVF.LanguageResource.Resources.lblARTCycle<div ng-class="sortClass"></div></th>
                                    <th st-ratio="7" ng-click="SortData('VitrificationNo')"> @PIVF.LanguageResource.Resources.plblFreezing<div ng-class="sortClass"></div></th>
                                    <th st-ratio="8" ng-click="SortData('VitriDate')">@PIVF.LanguageResource.Resources.plblFreezingDate / @PIVF.LanguageResource.Resources.plbltime<div ng-class="sortClass"></div></th>
                                    <th st-ratio="7" ng-click="SortData('ExpiryDate')">@PIVF.LanguageResource.Resources.plblpex / @PIVF.LanguageResource.Resources.plbltime<div ng-class="sortClass"></div></th>
                                    <th st-ratio="5" ng-click="SortData('TankDescription')">@PIVF.LanguageResource.Resources.plblTank<div ng-class="sortClass"></div></th>
                                    <th st-ratio="6" ng-click="SortData('CanistorDescription')">@PIVF.LanguageResource.Resources.lblCanister<div ng-class="sortClass"></div></th>
                                    <th st-ratio="6" ng-click="SortData('GlobletColorDescription')">@PIVF.LanguageResource.Resources.plblGlobletcolor<div ng-class="sortClass"></div></th>
                                    <th st-ratio="6" ng-click="SortData('GlobletSizeDescription')" ng-hide="true">@PIVF.LanguageResource.Resources.plblGlobletSize<div ng-class="sortClass"></div></th>
                                    <th st-ratio="6" ng-click="SortData('CryolockID')">@PIVF.LanguageResource.Resources.plblcryolock<div ng-class="sortClass"></div></th>
                                    <th st-ratio="6" ng-click="SortData('MediaUsed')">@PIVF.LanguageResource.Resources.plblmediaused<div ng-class="sortClass"></div></th>
                                    <th st-ratio="5" ng-click="SortData('LotNo')">@PIVF.LanguageResource.Resources.plbllot<div ng-class="sortClass"></div></th>
                                    <th st-ratio="7" ng-click="SortData('MediaExpiryDate')">@PIVF.LanguageResource.Resources.plblmediaex<div ng-class="sortClass"></div></th>
                                    <th st-ratio="8" ng-click="SortData('CellStageDescription')">@PIVF.LanguageResource.Resources.plblcell<div ng-class="sortClass"></div></th>
                                    <th st-ratio="4">@PIVF.LanguageResource.Resources.plblhistory</th>
                                    <th st-ratio="4" ng-click="SortData('CustomStatus')">@PIVF.LanguageResource.Resources.lblStatus<div ng-class="sortClass"></div></th>
                                </tr>
                            </thead>
                            <tbody style="max-height: 250px;">
                                <tr class="" ng-repeat="item in EmbryoDetails.ListVitriDetails |orderBy:SortColumn:reverseSort" ng-class="{trRed :item.ExpiryDate < TodayDate,trGreen :item.ExpiryDate >TodayDate && item.ExpiryDate <=DateAfter15Days }">
                                    <td st-ratio="3"><input type="checkbox" ng-click="AddSelectedPatient(item)" ng-model="item.selected"></td>
                                    <td st-ratio="6">{{item.EmbNumber}}</td>
                                    <td st-ratio="6" ng-hide="IsSinglePatient">{{item.MRNo}}</td>
                                    <td st-ratio="8" ng-hide="IsSinglePatient">{{item.PatientName}}</td>
                                    <td st-ratio="7">{{item.CycleCode}}</td>
                                    <td st-ratio="7">{{item.VitrificationNo}}</td>
                                    <td st-ratio="8">{{item.VitriDate | date :  "dd-MMM-yyyy"}}</br>{{item.VitriTime | date :  "HH:mm"}}</td>
                                    <td st-ratio="7">{{item.ExpiryDate | date :  "dd-MMM-yyyy"}}</br>{{item.ExpiryTime | date :  "HH:mm"}}</td>
                                    <td st-ratio="5">{{item.TankDescription}}</td>
                                    <td st-ratio="6">{{item.CanistorDescription}}</td>
                                    <td st-ratio="6">{{item.GlobletColorDescription}}</td>
                                    <td st-ratio="6" ng-hide="true">{{item.GlobletSizeDescription}}</td>
                                    <td st-ratio="6">{{item.CryoLockDescription}}</td>
                                    <td st-ratio="6">{{item.MediaUsed}}</td>
                                    <td st-ratio="5">{{item.LotNo}}</td>
                                    <td st-ratio="7">{{item.MediaExpiryDate| date :  "dd-MMM-yyyy"}}</td>
                                    <td st-ratio="8">{{item.CellStageDescription}}</td>
                                    <td st-ratio="4"><button class="btn btn_history" id="" value="" data-toggle="modal" data-target="#historymodal" ng-click="RedirectToHistoryEmb(item)" type="button" data-backdrop="static" data-keyboard="false"></button></td>
                                    <td st-ratio="4">{{item.CustomStatus}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>

                    <div class="row" style="margin-bottom:24px">
                        <div class="col-md-12 col-lg-12">
                            <label class="checkbox-inline p-l-25">
                                <input type="checkbox" id="" ng-model="RenewExpiryDatePatient.IsShowDiscard" ng-change="ShowDiscardChangeEmbryo(RenewExpiryDatePatient.IsShowDiscard,EmbryoBankOocyte.StatusID)" value="option1"> @PIVF.LanguageResource.Resources.plblshow
                            </label>
                            <label class="checkbox-inline p-l-25">
                                <span class="trcolorGreen glyphicon glyphicon-stop "></span>@PIVF.LanguageResource.Resources.plblNearingex
                            </label>
                            <label class="checkbox-inline p-l-25">
                                <span class="trcolorRed glyphicon glyphicon-stop "></span> @PIVF.LanguageResource.Resources.plblexpired
                            </label>
                        </div>

                    </div>

                    <h5 class="subTitle" style="margin-bottom:16px">@PIVF.LanguageResource.Resources.plblbank</h5>
                    <div style="min-height:150px; margin-bottom:12px; border-radius:10px" class="table-responsive table-bordered fixed_header">
                        <table class="table" style="min-width:2500px;">
                            <thead>
                                <tr>
                                    <th st-ratio="4" ng-show="!RenewExpiryDatePatient.IsShowDiscard">@PIVF.LanguageResource.Resources.lblSelect</th>
                                    <th st-ratio="12" ng-click="SortData('CryoNo')">@PIVF.LanguageResource.Resources.plblsperm.<div ng-class="sortClass"></div></th>
                                    <th st-ratio="8" ng-hide="IsSinglePatient" ng-click="SortData('MRNo')">@PIVF.LanguageResource.Resources.lblmrnoo<div ng-class="sortClass"></div></th>
                                    <th st-ratio="7" ng-hide="IsSinglePatient" ng-click="SortData('DonorDode')">@PIVF.LanguageResource.Resources.lblDonorCode<div ng-class="sortClass"></div></th>
                                    <th st-ratio="8" ng-hide="IsSinglePatient" ng-click="SortData('PatientName')">@PIVF.LanguageResource.Resources.lblPatientName<div ng-class="sortClass"></div></th>
                                    <th st-ratio="10" ng-click="SortData('SpremFreezingDate')">@PIVF.LanguageResource.Resources.plblVitrifiDate <br /> @PIVF.LanguageResource.Resources.plbltime<div ng-class="sortClass"></div></th>
                                    <th st-ratio="8" ng-click="SortData('ExpiryDate')">@PIVF.LanguageResource.Resources.plblpex <br /> @PIVF.LanguageResource.Resources.plbltime<div ng-class="sortClass"></div></th>
                                    <th st-ratio="5" ng-click="SortData('TankDescription')">@PIVF.LanguageResource.Resources.plblTank<div ng-class="sortClass"></div></th>
                                    <th st-ratio="5" ng-click="SortData('CanisterDescription')">@PIVF.LanguageResource.Resources.lblCanister<div ng-class="sortClass"></div></th>
                                    <th st-ratio="5" ng-click="SortData('CaneDescription')">@PIVF.LanguageResource.Resources.plblcane<div ng-class="sortClass"></div></th>
                                    <th st-ratio="5" ng-click="SortData('VialDescription')">@PIVF.LanguageResource.Resources.plblVials<div ng-class="sortClass"></div></th>
                                    <th st-ratio="5" ng-click="SortData('Volume')">@PIVF.LanguageResource.Resources.plblVolume<div ng-class="sortClass"></div></th>
                                    <th st-ratio="6" ng-click="SortData('SpermCount')">@PIVF.LanguageResource.Resources.plblTSC<div ng-class="sortClass"></div></th>
                                    <th st-ratio="6" ng-click="SortData('Motility')">@PIVF.LanguageResource.Resources.plblmotility<div ng-class="sortClass"></div></th>
                                    <th st-ratio="4">@PIVF.LanguageResource.Resources.plblhistory</th>
                                    <th st-ratio="5" ng-click="SortData('CustomStatus')">@PIVF.LanguageResource.Resources.lblStatus<div ng-class="sortClass"></div></th>
                                </tr>
                            </thead>
                            <tbody style="max-height: 250px;">
                                <tr ng-repeat="item in OocyteBankList  |orderBy:SortColumn:reverseSort " ng-class="{trRed :item.ExpiryDate < TodayDate,trGreen :item.ExpiryDate >TodayDate && item.ExpiryDate <=DateAfter15Days }">
                                    <td st-ratio="4" ng-show="!RenewExpiryDatePatient.IsShowDiscard">
                                        <input type="checkbox" ng-click="AddSelectedPatient(item)" ng-model="item.selected">
                                    </td>
                                    <td st-ratio="12">{{item.CryoNo}}</td>
                                    <td st-ratio="8" ng-hide="IsSinglePatient">{{item.MRNo}}</td>
                                    <td st-ratio="7" ng-hide="IsSinglePatient">{{item.DonorDode}}</td>
                                    <td st-ratio="8" ng-hide="IsSinglePatient">{{item.PatientName}}</td>
                                    <td st-ratio="10">{{item.SpremFreezingDate | date :"dd-MMM-yyyy"}} <br /><b>{{item.SpremFreezingTime | date:"HH:mm"}}</b></td>
                                    <td st-ratio="8">{{item.ExpiryDate | date :"dd-MMM-yyyy" }} <br /><b> {{item.ExpiryDate | date:"HH:mm" }}</b></td>
                                    <td st-ratio="5">{{item.TankDescription}}</td>
                                    <td st-ratio="5">{{item.CanisterDescription}}</td>
                                    <td st-ratio="5">{{item.CaneDescription}}</td>
                                    <td st-ratio="5">{{item.VialDescription}}</td>
                                    <td st-ratio="5">{{item.Volume}}</td>
                                    <td st-ratio="6">{{item.SpermCount}}</td>
                                    <td st-ratio="6">{{item.Motility}}</td>
                                    <td st-ratio="4"><button class="btn btn_history" type="button" id="" value="" data-toggle="modal" ng-click="RedirectToHistorySpermBank(item)" data-target="#historymodalSpermBank" data-backdrop="static" data-keyboard="false"></button></td>
                                    <td st-ratio="5">{{item.CustomStatus}}</td>
                            </tbody>
                        </table>
                    </div>

                    <div class="col-md-12 col-lg-12" style="margin-bottom:24px">
                        <label class="checkbox-inline p-l-25">
                            <input type="checkbox" id="" ng-model="RenewExpiryDatePatient.IsShowDiscard" ng-change="ShowDiscardChangeSpermBank(RenewExpiryDatePatient.IsShowDiscard,OocyteBank.StatusID)" value="option1"> @PIVF.LanguageResource.Resources.plblshowdis
                        </label>
                        <label class="checkbox-inline p-l-25">
                            <span class="trcolorGreen glyphicon glyphicon-stop "></span>@PIVF.LanguageResource.Resources.plblNearingex
                            <label class="checkbox-inline p-l-25">
                                <span class="trcolorRed glyphicon glyphicon-stop "></span>
                                @PIVF.LanguageResource.Resources.plblexpired
                            </label>
                    </div><div class="clearfix"></div>
                </div>


            </div>


            <div class="row" style="position: relative;
        z-index: 11; border-top: 1px solid #E2E3E5;">
                <div class="col-md-12 text-right m-t-10 m-b-10">
                    <button class="btn btn-primary" type="button" ng-show="!RenewExpiryDatePatient.IsShowDiscard" ng-click="RedirectToRenewDateModel()" value="" data-toggle="modal" data-target="{{Redirect}}" data-backdrop="static" data-keyboard="false">@PIVF.LanguageResource.Resources.plblRenew</button>
                    <button class="btn btn-default" type="button" ng-show="!RenewExpiryDatePatient.IsShowDiscard" ng-click="UpdateDiscardSpermBank()" value="" data-toggle="modal" data-target="{{RedirectToDiscard}}" data-backdrop="static" data-keyboard="false">@PIVF.LanguageResource.Resources.plblDiscard</button>
                    <button class="btn btn-default" type="button" ng-show="!RenewExpiryDatePatient.IsShowDiscard" ng-click="donateOocyte()" data-toggle="modal" data-target="donate" data-backdrop="static" data-keyboard="false">@PIVF.LanguageResource.Resources.plblTransport</button>
                    <button class="btn btn-default" type="button" ng-show="!RenewExpiryDatePatient.IsShowDiscard" ng-click="CheckPatientIsFemale()" data-toggle="modal" data-target="{{RedirectToDFR}}" data-backdrop="static" data-keyboard="false">@PIVF.LanguageResource.Resources.plblDFR</button>
                </div>
            </div>
        </div>


        <!-- History Modal for Sperm Bank Start -->
        <div class="modal fade" id="historymodalSpermBank" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title">@PIVF.LanguageResource.Resources.plblhistory</h4>
                    </div>
                    <div class="modal-body">
                        <!-- Previous Treatment History Start -->
                        <div class="row">
                            <!-- <section class="col-lg-12"><h5 class="subTitle">Grid Title</h5></section> -->
                            <div class="col-lg-12">
                                @*<div class="table-responsive" style="max-height:300px; min-height:300px;" ng-scrollbars ng-scrollbars-config="config">*@
                                <div class="table-responsive" style="max-height:300px; min-height:300px;">
                                    <table class="table table-hover table-striped table-bordered normalLabel" id="" style="width:1000px">
                                        <thead>
                                            <tr>
                                                <th>@PIVF.LanguageResource.Resources.plblTransactiontype</th>
                                                <th>@PIVF.LanguageResource.Resources.lblDate</th>
                                                <th>@PIVF.LanguageResource.Resources.plbltime</th>
                                                <th>@PIVF.LanguageResource.Resources.plblpex</th>
                                                <th>@PIVF.LanguageResource.Resources.plblexptime</th>
                                                <th>@PIVF.LanguageResource.Resources.plblTank</th>
                                                <th>@PIVF.LanguageResource.Resources.lblCanister</th>
                                                <th>@PIVF.LanguageResource.Resources.plblcane</th>
                                                <th>@PIVF.LanguageResource.Resources.plblVials</th>
                                                <th>@PIVF.LanguageResource.Resources.plblVolume</th>
                                                <th>@PIVF.LanguageResource.Resources.plblEmbryologist</th>
                                                <th>@PIVF.LanguageResource.Resources.plblwitness</th>
                                                <th>@PIVF.LanguageResource.Resources.plblclinician</th>
                                                <th>@PIVF.LanguageResource.Resources.lblCycleCode</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="item in lstFreezingHistory" class="">
                                                <td>{{item.CustomStatus}}</td>
