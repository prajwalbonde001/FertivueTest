<section class="form_content" ng-controller="PatientDashboardCtrl" ng-init="FormLoad()">


    <div class="tabDiv">
        <h3 class="card-title">@PIVF.LanguageResource.Resources.lblDashboardOverview</h3>
        <!-- Tabs Navigation -->
        <ul class="nav nav-tabs">
            <li ng-class="{ active: selectedTab === 'year' }">
                <a ng-click="showTab('year')">@PIVF.LanguageResource.Resources.lblYear</a>
            </li>
            <li ng-class="{ active: selectedTab === 'month' }">
                <a ng-click="showTab('month')">@PIVF.LanguageResource.Resources.lblMonth</a>
            </li>
            <li ng-class="{ active: selectedTab === 'day' }">
                <a ng-click="showTab('day')">@PIVF.LanguageResource.Resources.lblDay</a>
            </li>
            <li ng-class="{ active: selectedTab === 'custom' }">
                <a ng-click="showTab('custom')">
                    @PIVF.LanguageResource.Resources.lblCustom
                    <span class="custom-date-display" ng-if="chartCustomFdate && chartCustomTdate">
                        {{chartCustomFdate}} - {{chartCustomTdate}}
                    </span>
                </a>
            </li>
        </ul>
    </div>

    <div class="row">

        <div class="col-md-12 col-lg-12 col-sm-12" id="cycle">


            @* ------------------------------New UI--------------------------- *@

            <div class="row g-3" style="margin-top: 20px;">




                <!-- Repeatable Card -->
                <div class="col-md-2 col-sm-6" ng-repeat="item in PatientUnderStimulation" ng-click="changeSelectedTab('PatientUnderStimulation' , item.TotalPatient)">
                    <div class="d-flex align-items-center" ng-class="{'dashboard-card': selectedDashboardTab !== 'PatientUnderStimulation' , 'dashboard-cardActive': selectedDashboardTab === 'PatientUnderStimulation'}">
                        <div class="icon-box me-3">
                            <img ng-show="selectedDashboardTab === 'PatientUnderStimulation'" src="../images/SchedulingS.svg" alt="SchedulingS">
                            <img ng-show="selectedDashboardTab !== 'PatientUnderStimulation'" src="../images/SchedulingNS.svg" alt="SchedulingNS">
                        </div>
                        <div class="text-box">
                            <h3 ng-class="{'cardS': selectedDashboardTab !== 'PatientUnderStimulation' , 'cardSActive': selectedDashboardTab === 'PatientUnderStimulation'}">{{item.TotalPatient}}</h3>
                            <h6 class="cardH">@PIVF.LanguageResource.Resources.lblPatientUnderStimulation</h6>
                        </div>
                    </div>
                </div>

                <div class="col-md-2 col-sm-6" ng-repeat="item in PatientTrigger" ng-click="changeSelectedTab('Trigger' , item.TotalTrigger)">
                    <div class="d-flex align-items-center" ng-class="{'dashboard-card': selectedDashboardTab !== 'Trigger' , 'dashboard-cardActive': selectedDashboardTab === 'Trigger'}">
                        <div class="icon-box me-3">
                            <img ng-show="selectedDashboardTab === 'Trigger'" src="../images/SchedulingS.svg" alt="SchedulingS">
                            <img ng-show="selectedDashboardTab !== 'Trigger'" src="../images/SchedulingNS.svg" alt="SchedulingNS">
                        </div>
                        <div class="text-box">
                            <h3 ng-class="{'cardS': selectedDashboardTab !== 'Trigger' , 'cardSActive': selectedDashboardTab === 'Trigger'}">{{item.TotalTrigger}}</h3>
                            <h6 class="cardH">@PIVF.LanguageResource.Resources.lblTriggerHCGgiven</h6>
                        </div>
                    </div>
                </div>


                <div class="col-md-2 col-sm-6" ng-click="changeSelectedTab('EmbryologyDay' , GetAllDayPatientList[0].TotalCount || 0 )">
                    <div class="d-flex align-items-center" ng-class="{'dashboard-card': selectedDashboardTab !== 'EmbryologyDay' , 'dashboard-cardActive': selectedDashboardTab === 'EmbryologyDay'}">
                        <div class="icon-box me-3">
                            <img ng-show="selectedDashboardTab === 'EmbryologyDay'" src="../images/SchedulingS.svg" alt="SchedulingS">
                            <img ng-show="selectedDashboardTab !== 'EmbryologyDay'" src="../images/SchedulingNS.svg" alt="SchedulingNS">
                        </div>
                        <div class="text-box">
                            <h3 ng-class="{'cardS': selectedDashboardTab !== 'EmbryologyDay' , 'cardSActive': selectedDashboardTab === 'EmbryologyDay'}">{{GetAllDayPatientList[0].TotalCount || 0 }}</h3>
                            <h6 class="cardH">@PIVF.LanguageResource.Resources.lblEmbryologyDay</h6>
                        </div>
                    </div>
                </div>

                <div class="col-md-2 col-sm-6" ng-repeat="item in PregnancyTest" ng-click="changeSelectedTab('PregnancyTest' , item.PregnancyTest)">
                    <div class="d-flex align-items-center" ng-class="{'dashboard-card': selectedDashboardTab !== 'PregnancyTest' , 'dashboard-cardActive': selectedDashboardTab === 'PregnancyTest'}">
                        <div class="icon-box me-3">
                            <img ng-show="selectedDashboardTab === 'PregnancyTest'" src="../images/SchedulingS.svg" alt="SchedulingS">
                            <img ng-show="selectedDashboardTab !== 'PregnancyTest'" src="../images/SchedulingNS.svg" alt="SchedulingNS">
                        </div>
                        <div class="text-box">
                            <h3 ng-class="{'cardS': selectedDashboardTab !== 'PregnancyTest' , 'cardSActive': selectedDashboardTab === 'PregnancyTest'}">{{item.PregnancyTest}}</h3>
                            <h6 class="cardH">@PIVF.LanguageResource.Resources.lblPregnancyTest</h6>
                        </div>
                    </div>
                </div>


                <div class="col-md-2 col-sm-6" ng-repeat="item in PregnancyUltrasound" ng-click="changeSelectedTab('PregnancyUltrasound' , item.PregnancyUltrasound)">
                    <div class="d-flex align-items-center" ng-class="{'dashboard-card': selectedDashboardTab !== 'PregnancyUltrasound' , 'dashboard-cardActive': selectedDashboardTab === 'PregnancyUltrasound'}">
                        <div class="icon-box me-3">
                            <img ng-show="selectedDashboardTab === 'PregnancyUltrasound'" src="../images/SchedulingS.svg" alt="SchedulingS">
                            <img ng-show="selectedDashboardTab !== 'PregnancyUltrasound'" src="../images/SchedulingNS.svg" alt="SchedulingNS">
                        </div>
                        <div class="text-box">
                            <h3 ng-class="{'cardS': selectedDashboardTab !== ' PregnancyUltrasound' , 'cardSActive': selectedDashboardTab === 'PregnancyUltrasound'}">{{item.PregnancyUltrasound}}</h3>
                            <h6 class="cardH">@PIVF.LanguageResource.Resources.lblvPregnancyUltrasound</h6>
                        </div>
                    </div>
                </div>

                <div class="col-md-2 col-sm-6" ng-repeat="item in PregnancyOutcome" ng-click="changeSelectedTab('PregnancyOutcome' , item.PregnancyOutcome)">
                    <div class="d-flex align-items-center" ng-class="{'dashboard-card': selectedDashboardTab !== 'PregnancyOutcome' , 'dashboard-cardActive': selectedDashboardTab === 'PregnancyOutcome'}">
                        <div class="icon-box me-3">
                            <img ng-show="selectedDashboardTab === 'PregnancyOutcome'" src="../images/SchedulingS.svg" alt="SchedulingS">
                            <img ng-show="selectedDashboardTab !== 'PregnancyOutcome'" src="../images/SchedulingNS.svg" alt="SchedulingNS">
                        </div>
                        <div class="text-box">
                            <h3 ng-class="{'cardS': selectedDashboardTab !== 'PregnancyOutcome' , 'cardSActive': selectedDashboardTab === 'PregnancyOutcome'}">{{item.PregnancyOutcome}}</h3>
                            <h6 class="cardH">@PIVF.LanguageResource.Resources.lblvPregnancyOutcome</h6>
                        </div>
                    </div>
                </div>

                <!-- End of Repeatable Card -->
            </div>



            <div class="DashboardTableContainer">

                <div id="content" class="col-md-12" style="display:flex; flex-direction:column; gap : 18px;">
                    <h3 class="cardS"> {{selectedDashboardTabShowOnDashboard}} @PIVF.LanguageResource.Resources.lblvPatientList ( {{selectedDashboardTabValue}} )</h3>

                    <div class="tabDiv dayTabs" ng-show="selectedDashboardTab === 'EmbryologyDay'">
                        <!-- Tabs Navigation -->
                        <ul class="nav nav-tabs">
                            <li ng-class="{ active: selectedDayTab === 'AllDays' }">
