<script type="text/ng-template" id="exampleModal">
    <div class="custom-modal-content">
        <div class="custom-modal-header">
            <h5 class="custom-modal-title">@PIVF.LanguageResource.Resources.lblvSelectDates</h5>
        </div>

        <div class="custom-modal-body">
            <div class="form-group">
                <label for="fromDate" class="custom-label">@PIVF.LanguageResource.Resources.lblFromDate : </label>
                <div class="custom-input-group">
                    <input type="text"
                           id="fromDate"
                           class="custom-input"
                           placeholder="@PIVF.LanguageResource.Resources.msgSelectfromdate"
                           uib-datepicker-popup="{{format}}"
                           ng-model="FromDate"
                           is-open="isFromDatePickerOpen"
                           datepicker-options="FromDateOptions"
                           ng-focus="isFromDatePickerOpen=true" />
                    <button class="custom-calendar-btn" type="button" ng-click="isFromDatePickerOpen=true">
                        <i class="fa fa-calendar"></i>
                    </button>
                </div>
            </div>

            <div class="form-group">
                <label for="toDate" class="custom-label">@PIVF.LanguageResource.Resources.lblToDate : </label>
                <div class="custom-input-group">
                    <input type="text"
                           id="toDate"
                           class="custom-input"
                           placeholder="@PIVF.LanguageResource.Resources.msgSelecttodate"
                           uib-datepicker-popup="{{format}}"
                           ng-model="ToDate"
                           is-open="isToDatePickerOpen"
                           datepicker-options="ToDateOptions"
                           ng-focus="isToDatePickerOpen=true" />
                    <button class="custom-calendar-btn" type="button" ng-click="isToDatePickerOpen=true">
                        <i class="fa fa-calendar"></i>
                    </button>
                </div>
            </div>
        </div>

        <div class="custom-modal-footer">
            <button class="custom-btn-primary" ng-click="close()">@PIVF.LanguageResource.Resources.lblvSubmit</button>
        </div>
    </div>
</script>