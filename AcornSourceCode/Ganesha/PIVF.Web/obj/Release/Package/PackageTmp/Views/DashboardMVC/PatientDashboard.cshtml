<style>
        .card-body {
            border: 1px solid gray;
            border-radius: 10px;
            margin: 2%;
        }

        .headB {
            border-bottom: 1px solid gray;
            margin: 20px;
        }


        .surgery-dashboard {
            font-family: Montserrat;
            font-size: 18px;
            font-weight: 700;
            line-height: 21.94px;
            text-align: left;
            color: #232323;
            padding-left: 20px;
        }

        .surgery-row {
            background-color: #E5E5E5;
            border-radius: 15px;
            height: 80px;
            display: flex;
            align-items: center;
            justify-content: space-between;
            margin: 10px 0;
            cursor: pointer;
            /*border:1px solid gray;*/
        }

        .text-number {
            font-family: Montserrat;
            /* font-size: 2rem; */
            font-weight: 700;
            line-height: 24.38px;
            text-align: left;
        }

        .char-number {
            font-family: Montserrat;
            font-size: 0.7rem;
            font-size: 16px;
            font-weight: 500;
            line-height: 19.5px;
            text-align: left;
        }


        .surgery-images img {
            width: 100%;
            max-width: 50px;
        }

        .sergery_contains_two {
            display: flex;
            justify-content: center;
            gap: 20px;
        }




        .form_Content1 {
            border: 1px solid #c3c3c3;
            padding: 0px 10px 20px 10px;
            margin-top: 10px;
            background: #ffffff;
            border-radius: 4px;
            position: relative;
            height: auto;
        }





        .headB {
            display: flex;
            justify-content: space-between;
            align-items: center;
        }


        .headBox {
            display: flex;
            gap: 10px;
        }








        .nav-tabs {
            display: flex;
            justify-content: flex-end; /* Align tabs to the right */
            border: none !important;
        }


            .nav-tabs .active a {
                /*color: #fff !important;
        background-color: #df7d36 !important;*/
                background: #EFF5F7 !important;
                color: #5895A6 !important;
                border-radius: 12px !important;
                border: 1px solid #E4EEF1 !important;
                font-weight: bold;
                padding: 10px 30px !important;
            }


            .nav-tabs li a {
                /*color: #000 !important;
        background-color: #f8f9fa !important;
        border-radius: 5px;*/
                background: #FFFFFF !important;
                color: #232323 !important;
                border-radius: 12px !important;
                text-decoration: none;
                border: 1px solid #E4EEF1 !important;
                padding: 10px 30px !important;
            }




        .tabDiv {
            /*padding-right:2%;
          padding-left: 2%;*/
            display: flex;
            justify-content: space-between;
            align-items: center;
        }











        .modal-content {
            max-width: 500px !important;
        }

        /* Modal Container */
        .custom-modal-content {
            background-color: #ffffff;
            border-radius: 12px;
            box-shadow: 0 6px 20px rgba(0, 0, 0, 0.1);
            padding: 20px;
            max-width: 500px;
            margin: auto;
            font-family: 'Arial', sans-serif;
        }

        /* Modal Header */
        .custom-modal-header {
            border-bottom: 1px solid #f0f0f0;
            padding-bottom: 10px;
            margin-bottom: 20px;
        }

        .custom-modal-title {
            font-size: 18px;
            font-weight: bold;
            color: #333333;
            text-align: center;
        }

        /* Labels */
        .custom-label {
            font-size: 14px;
            font-weight: bold;
            color: #555555;
            margin-bottom: 5px;
            display: block;
        }

        /* Input Group */
        .custom-input-group {
            display: flex;
            align-items: center;
            border: 1px solid #ddd;
            border-radius: 6px;
            overflow: hidden;
            background-color: #f9f9f9;
        }

        .custom-input {
            flex: 1;
            border: none;
            padding: 10px 12px;
            font-size: 14px;
            color: #333333;
            background-color: transparent;
            outline: none;
        }

            .custom-input::placeholder {
                color: #999999;
            }

        .custom-calendar-btn {
            background-color: #eeeeee;
            border: none;
            padding: 10px 12px;
            cursor: pointer;
            color: #555555;
            font-size: 16px;
            outline: none;
            transition: background-color 0.3s;
        }

            .custom-calendar-btn:hover {
                background-color: #dddddd;
            }

        /* Modal Footer */
        .custom-modal-footer {
            border-top: 1px solid #f0f0f0;
            margin-top: 20px;
            text-align: center;
            padding-top: 10px;
        }

        .custom-btn-primary {
            background-color: #0a9c92;
            border: none;
            color: #ffffff;
            padding: 10px 20px;
            font-size: 14px;
            border-radius: 6px;
            cursor: pointer;
            transition: background-color 0.3s;
        }

            .custom-btn-primary:hover {
                background-color: #df7d36;
            }




        .custom-date-display {
            font-weight: bolder;
            font-style: italic;
        }












        .dayTabs .nav-tabs {
            display: flex;
            justify-content: flex-end; /* Align tabs to the right */
            border: none !important;
        }

            .dayTabs .nav-tabs .active a {
                background: #EFF5F7 !important;
                color: #5895A6 !important;
                border-radius: 10px !important;
                border: 1px solid #5895A6 !important;
                font-weight: bold;
                padding: 8px 16px !important;
                box-shadow: 4px 4px 8px 0px #5895A60F;
            }

            .dayTabs .nav-tabs li a {
                color: #9E9E9E !important;
                border-radius: 10px !important;
                text-decoration: none;
                padding: 8px 16px !important;
                border: 1px solid #E2E3E5 !important;
                background: #FAFAFA !important;
            }









        .custom-table-container {
            /*max-height: 518px;*/
            overflow-y: auto;
            overflow-x: auto;
            width: 100%;
            height: calc(100vh - 565px);
        }





        .embroDays-custom-table-container {
            /*max-height: 518px;*/
            overflow-y: auto;
            overflow-x: auto;
            width: 100%;
            height: calc(100vh - 612px);
        }

        .unique-table-class {
            width: 100%;
            border-collapse: collapse;
            table-layout: auto;
        }





            .unique-table-class th,
            .unique-table-class td {
                /*border: 1px solid gray;*/
                padding: 8px;
                /*text-align: center;*/
                font-size: 12px;
                white-space: nowrap; /* Prevent word breaking */
            }

            .unique-table-class th {
                /*background-color: #f2f2f2;*/
                font-weight: bold;
                position: sticky;
                top: 0;
                z-index: 1;
                white-space: nowrap; /* Prevent word breaking */
                border: none !important;
            }


            .unique-table-class > thead > tr {
                background: #F6F9FB;
                color: #5895a6;
                border: none !important;
                border-radius: 12px !important;
                position: sticky;
                top: 2px;
                z-index: 1;
                white-space: nowrap;
            }


            .unique-table-class > tbody > tr > td {
                background: #FFFFFF !important;
                border-bottom: 1px solid #F4F4F5 !important;
                color: #353535 !important;
                border-top: none !important;
                white-space: nowrap;
            }



        .no-table-styles {
            max-height: none !important;
            overflow-y: visible !important;
            overflow-x: visible !important;
            border: none !important;
        }



