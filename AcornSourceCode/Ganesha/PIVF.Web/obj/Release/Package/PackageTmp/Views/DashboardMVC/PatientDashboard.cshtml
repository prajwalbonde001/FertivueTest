                            </li>
                            <li ng-class="{ active: selectedDayTab === 'Day1' }">
                                <a href="" ng-click="showDayTab('Day1')">@PIVF.LanguageResource.Resources.lblDay 1 ({{Day1Patient[0].Day1}})</a>
                            </li>
                            <li ng-class="{ active: selectedDayTab === 'Day2' }">
                                <a href="" ng-click="showDayTab('Day2')">@PIVF.LanguageResource.Resources.lblDay 2 ({{Day2Patient[0].Day2}})</a>
                            </li>
                            <li ng-class="{ active: selectedDayTab === 'Day3' }">
                                <a href="" ng-click="showDayTab('Day3')">@PIVF.LanguageResource.Resources.lblDay 3 ({{Day3Patient[0].Day3}})</a>
                            </li>
                            <li ng-class="{ active: selectedDayTab === 'Day4' }">
                                <a href="" ng-click="showDayTab('Day4')">@PIVF.LanguageResource.Resources.lblDay 4 ({{Day4Patient[0].Day4}})</a>
                            </li>
                            <li ng-class="{ active: selectedDayTab === 'Day5' }">
                                <a href="" ng-click="showDayTab('Day5')">@PIVF.LanguageResource.Resources.lblDay 5 ({{Day5Patient[0].Day5}})</a>
                            </li>
                            <li ng-class="{ active: selectedDayTab === 'Day6' }">
                                <a href="" ng-click="showDayTab('Day6')">@PIVF.LanguageResource.Resources.lblDay 6 ({{Day6Patient[0].Day6}})</a>
                            </li>
                        </ul>


                    </div>

                    @*<div class="table-responsive fixed_header" id="tableToExport">
                        <table class="table table-hover table-striped custom-table-container">*@
                    <div id="tableToExport" ng-class="{'custom-table-container': selectedDashboardTab !== 'EmbryologyDay' , 'embroDays-custom-table-container': selectedDashboardTab === 'EmbryologyDay'}">
                        <table id="data-table" class="table table-hover table-striped  unique-table-class">
                            <thead>
                                <tr>
                                    <th>@PIVF.LanguageResource.Resources.lblMRNO .</th>
                                    <th>@PIVF.LanguageResource.Resources.lblPatientName</th>
                                    <th>@PIVF.LanguageResource.Resources.lblvAGE</th>
                                    <th>@PIVF.LanguageResource.Resources.lblGender</th>
                                    <th>@PIVF.LanguageResource.Resources.lblPartnerName</th>
                                    @* <th>Cycle code</th>*@
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="Item in PatientList">
                                    <td>{{Item.MRNo  && Item.MRNo != 0 ? Item.MRNo : '-' }}</td>
                                    <td>{{Item.PatientName  && Item.PatientName != 0 ? Item.PatientName : '-' }}</td>
                                    <td>{{ Item.Age && Item.Age != 0 ? Item.Age : '-' }}</td>
                                    <td>{{ Item.Gender && Item.Gender != 0 ? Item.Gender : '-' }}</td>
                                    <td>{{ Item.PartnerName && Item.PartnerName != 0 ? Item.PartnerName : '-' }}</td>
                                    @*<td>{{ Item.ContactNo1 && Item.ContactNo1 != 0 ? Item.ContactNo1 : '-' }}</td>*@
                                    <td style="display:flex; justify-content: flex-end; padding-right:20px"><img src="../images/dashboardEye.svg" style="cursor:pointer;" ng-click="EditPatient(Item)" alt="dashboardEye"></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>

                <div class="col-md-12 text-right m-t-10" style="padding: 12px; border-top: 1px solid #E2E3E5;" ng-show="PatientList.length">
                    <button type="button" class="btn btn-primary" ng-click="downloadReportWithPreview()">@PIVF.LanguageResource.Resources.btnPrint</button>
                </div>

            </div>











