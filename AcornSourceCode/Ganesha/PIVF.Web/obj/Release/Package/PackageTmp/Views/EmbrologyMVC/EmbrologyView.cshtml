    .tooltip-container {
        position: relative; /* Ensures the arrow is positioned correctly */
        border: 1px solid rgba(222, 234, 237, 1);
        border-radius: 12px;
        font-weight: 400;
        z-index: 15;
        background: #fff;
        padding: 16px;
        box-shadow: 2px 2px 10px rgba(0, 0, 0, 0.1);
        width: 200px;
        text-align: left;
    }

        /* Tooltip Arrow */
        .tooltip-container::before {
            content: "";
            position: absolute;
            top: -20px; /* Moves arrow above the card */
