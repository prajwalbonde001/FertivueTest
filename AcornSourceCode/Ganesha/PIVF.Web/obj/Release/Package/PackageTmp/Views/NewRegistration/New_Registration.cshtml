                                            <canvas ng-hide="true" id="snapshot" width="300" height="300"></canvas>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-offset-3 col-sm-6">
                                                <div class="form-group text-center">
                                                    <button type="button" class="btn btn-primary  m-t-10" ng-click="makeSnapshot();addFor = 'partner'"><span><i class="fa fa-camera-retro"></i></span>@PIVF.LanguageResource.Resources.btnCapture</button>
