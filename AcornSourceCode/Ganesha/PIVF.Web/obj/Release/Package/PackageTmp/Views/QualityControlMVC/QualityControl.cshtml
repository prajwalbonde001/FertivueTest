            <button class="btn btn-default" type="button" id="" value="" ng-click="Cancel()">@PIVF.LanguageResource.Resources.btnCancel</button>@*<button class="btn btn-primary" type="button" id="" value="" tabindex="87">Cancel</button>*@
        </div>
