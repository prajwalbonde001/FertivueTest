
@{
    Layout = null;
}
<div formname="" ng-controller="QualityControlCtrl" ng-init="LoadData()">


    <div class="form_Content qualityCtrl" style="min-height:100px">
        <form name="frmQualityControl" novalidate autocomplete="off">
            @*m-h-390*@
            <div class="row form-horizontal">
                <div class="col-md-3 col-lg-3">
                    <div class="form-group">
                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for=""> @PIVF.LanguageResource.Resources.lblDate</label>
                        <div class="col-sm-12 col-md-12 col-lg-12">
                            <div class="input-group date" ng-class="{'Invalid':frmQualityControl.Date.$invalid && frmQualityControl.Date.$dirty }">
                                <input type="text" class="form-control" id="txtDate" name="txtDate" placeholder="dd-MMM-yyyy" ng-required="true" ng-model="QualityControl.Date" aria-describedby="" uib-datepicker-popup="{{format}}" show-button-bar="false" is-open="popup1.opened" datepicker-options="dateOptions" alt-input-formats="altInputFormats" tabindex="">
                                <span class="input-group-addon" id="" ng-click="open1()"><i class="fa fa-calendar" aria-hidden="true"></i></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 col-lg-3">
                    <div class="form-group">
                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblTime</label>
                        <div class="col-sm-12 col-md-12 col-lg-12">
                            <div class="input-group date" ng-class="{'Invalid':frmQualityControl.Time.$invalid && frmQualityControl.Time.$dirty }">
                                <div uib-timepicker id="txtTime" name="txtTime" class="timeCss" ng-required="true" ng-model="QualityControl.Time" hour-step="1" minute-step="5" max="maxTime" show-meridian="false" show-spinners="false"
                                     show-seconds="false" tabindex="2"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 col-lg-3">
                    <div class="form-group">
                        <label class="col-sm-12 col-md-12 col-lg-12 control-label" for="">@PIVF.LanguageResource.Resources.lblParameter</label>
                        @*<select class="form-control " id="ddlParameter" name="ddlParameter" ng-required="true" ng-class="{'select':frmQualityControl.ddlParameter.$dirty && QualityControl.ParameterID==0}"
                                     ng-model="QualityControl.ParameterID"
                                    ng-options="item.ID as item.Description for item in ParameterList">
                            </select>*@
                        @*ng-class="{'select':IsValidIndication}"*@

                        <div class="col-md-12 col-lg-12 m-b-7">
                            <div isteven-multi-select input-model="ParameterList"
                                 output-model="QualityControl.ParameterSelected"
                                 id="ddlParameter" name="ddlParameter"
                                 button-label="Description"
                                 class="resultDataSelect"
                                 ng-required="true"
                                 item-label="Description"
                                 tick-property="ticked"
                                 tabindex="19"
                                 output-properties="ID"
                                 max-height="300px"
                                 max-labels="2"
                                 on-item-click="PaClick(data)"
                                 on-select-all="PaSelectAll()"
                                 on-select-none="PaSelectNone()">
                            </div>
                        </div>
                    </div>
                </div>
            </div>


