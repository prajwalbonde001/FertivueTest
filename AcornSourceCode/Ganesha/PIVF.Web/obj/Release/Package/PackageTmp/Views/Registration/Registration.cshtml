@{ Layout = null;}
<style>
    .box_Individual {
        width: 100% !important;
    }

    .croparea {
        background: #E4E4E4;
        overflow: hidden;
        width: 350px;
        height: 350px;
    }
</style>
<form name="frmRegistration" ng-controller="RegistrationController" ng-init="formLoad()" nonvalidate>
    <div id="page_patientRegistration">
        <div class="row">
            <div class="col-md-2 col-lg-1 m-b-1 wizard-left">
                <div class="stepwizard">
                    <div class="stepwizard-row">
                        <div class="stepwizard-step stepwizard-step1">
                            <button type="button" class="btn btn-square" ng-class="info?'active':''" ng-click="refInfo=false;info=true;addInfo=false">1</button><span class="txtProTitle">@PIVF.LanguageResource.Resources.lblPatientInformation @*Patient Info*@</span>
                        </div>
                    </div>
                    <div class="stepwizard-row">
                        <div class="stepwizard-step">
                            <button type="button" class="btn btn-square" ng-class="addInfo?'active':''" ng-click="refInfo=false;info=false;addInfo=true">2</button><span class="txtProTitle">@PIVF.LanguageResource.Resources.lblAdditionalInformation @*Additional Info*@</span>
                        </div>
                    </div>
                    <div class="stepwizard-row">
                        <div class="stepwizard-step" ng-class="{'stepwizard-step3':Patient.IsReferredPatient}" ng-hide="RegType==3 || RegType==4">
                            <button type="button" ng-hide="!Patient.IsReferredPatient" class="btn btn-square" ng-class="refInfo?'active':''" ng-click="refInfo=true;info=false;addInfo=false">3</button><span class="txtProTitle" ng-hide="!Patient.IsReferredPatient">@PIVF.LanguageResource.Resources.lblReferralInformation @*Referral Info*@</span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-10 col-lg-11 m-b-11">
                <div class="row form-horizontal">
                    <div class="col-md-6 col-lg-6">
                        <div class="form-group ">
                            <label class="col-md-12 col-sm-12 col-lg-12 control-label p-r-10">
                                <span class="text-Red">* </span> @PIVF.LanguageResource.Resources.lblRegistrationType :  @*Registration Type :*@
                            </label><div class="visible-md clear"></div>
                            <div class="col-md-12 col-sm-12 col-lg-12">
                                <label class="radio-inline control-label p-t-0" ng-show="IsAuto">
                                    <input type="radio" ng-model="RegType" name="rdbReType" id="rdbReType1" ng-value="1" ng-change="chageRegType(RegType)" tabindex="1"> @PIVF.LanguageResource.Resources.lblCouple  @*Couple*@
                                </label>
                                <label class="radio-inline control-label p-t-0" ng-show="IsManual || IsAuto">
                                    <input type="radio" ng-model="RegType" name="rdbReType" id="rdbReType2" ng-value="2" ng-change="chageRegType(RegType)" tabindex="2">@PIVF.LanguageResource.Resources.lblIndividual  @*Individual*@
                                </label>
                                <label class="radio-inline control-label p-t-0" ng-show="IsAuto">
                                    <input type="radio" ng-model="RegType" name="rdbReType" id="rdbReType3" ng-value="3" ng-change="chageRegType(RegType)" tabindex="3">@PIVF.LanguageResource.Resources.lblDonor @*Donor*@
                                </label>
                                <label class="radio-inline control-label p-t-0" ng-show="IsAuto">
                                    <input type="radio" ng-model="RegType" name="rdbReType" id="rdbReType4" ng-value="4" ng-change="chageRegType(RegType)" tabindex="4">@PIVF.LanguageResource.Resources.lblSurrogate @*Surrogate*@
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-6 p-b-10 m-t-20 Tabresp-mb-25" ng-hide="RegType==3 || RegType==4">
                        @*desc-mb-15*@
                        <label class="checkbox-inline control-label">
                            <input type="checkbox" id="chkSpecReg" ng-model="Patient.IsSpecialReg" tabindex="5">@PIVF.LanguageResource.Resources.lblSpecialRegistration @*Special Registration*@
                        </label>
                        <label class="checkbox-inline control-label">
                            <input type="checkbox" id="chkRefferedPat" ng-model="Patient.IsReferredPatient" ng-click="FormChange(Patient.IsReferredPatient)" tabindex="6">@PIVF.LanguageResource.Resources.lblReferralPatient @*Referred Patient*@      @*ng-change="!Patient.IsReferredPatient?(info=true):0"   commented by Nayan Kamble and ng-click added*@
                        </label> 
                    </div>
                    <div class="col-md-6 col-lg-3 m-b-10" ng-show="RegType==3">
                        <div class="form-group ">
                            <label class="col-md-12 col-sm-12 col-lg-12 control-label">@PIVF.LanguageResource.Resources.lblAgent @*Agent*@</label>
                            <div class="col-md-12 col-sm-12 col-lg-12">
                                <select class="form-control" name="ddlAgent" ng-model="Patient.AgentID" ng-options="i.ID as i.Description for i in AgentList"></select>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-3 m-b-10" ng-show="RegType==4">
                        <div class="form-group ">
                            <label class="col-md-12 col-sm-12 col-lg-12 control-label">@PIVF.LanguageResource.Resources.lblAgency @*Agency*@</label>
                            <div class="col-md-12 col-sm-12 col-lg-12">
                                <select class="form-control" name="ddlAgency" ng-model="Patient.AgencyID" ng-options="i.ID as i.Description for i in AgencyList"></select>
                            </div>

                        </div>
                    </div>
                   
                    <div class="col-lg-6 col-md-6" ng-class="RegType==2 || RegType==3 || RegType==4 ?'box_Individual':''" ng-hide="addInfo || refInfo "> @*|| refInfo*@
                        <div class="row form_Content">
                            <div class="col-lg-12">
                                <h5 class="subTitle">@PIVF.LanguageResource.Resources.lblPatientInformation @*Patient Information*@</h5>
                            </div>


                            <div class="col-lg-offset-4 col-lg-4 col-md-12 text-center">
                                <div class="patient_pic">
                                    <button type="button" class="glyphicon glyphicon-trash imgEdit" style="top:10px;right:-8px;" title="Delete Profile Image" data-toggle="modal" data-target="#modPhotoDelete" ng-disabled="(Patient.strPhoto)==null" ng-click="fromGallery=false;fromWebCam=false;addFor ='patient';isAttach=false"></button>
                                    <button type="button" class="glyphicon glyphicon-pencil imgEdit" title="Edit Profile Image" data-toggle="modal" data-target="#modPhoto" ng-click="fromGallery=false;fromWebCam=false;addFor ='patient';isAttach=false"></button>
                                    <img src="{{Patient.strPhoto}}" class="profile" alt="">
                                </div>
                            </div><div class="clear"></div>

                            @*Begin::Added by AniketK on 08July2019*@
                            <div class="col-md-6 col-lg-4" ng-show="IsManual">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label"><span class="text-Red">* </span>MRNo</label>
                                    <div class="col-md-12 col-sm-12 col-lg-12" ng-class="{'Invalid':frmRegistration.txtMRNo.$invalid && frmRegistration.txtMRNo.$dirty }">
                                        <input type="text" class="form-control" name="txtMRNo" ng-model="Patient.MRNo" maxlength="20" multi-validation="AlphaArrowBkSpDelSp" ng-required="true" tabindex="6">
                                    </div>
                                </div>
                            </div><div class="clear"></div>
                            @*End::Added by AniketK on 08July2019*@

                            <div class="col-md-6 col-lg-4">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label"><span class="text-Red">* </span> @PIVF.LanguageResource.Resources.lblFirstName @*First Name*@</label>
                                    <div class="col-md-12 col-sm-12 col-lg-12" ng-class="{'Invalid':frmRegistration.txtFirstName.$invalid && frmRegistration.txtFirstName.$dirty }">
                                        <input type="text" class="form-control Capitalize" name="txtFirstName" ng-model="Patient.FirstName" maxlength="20" multi-validation="AlphaArrowBkSpDelSp" ng-required="true" tabindex="7" onkeypress='return (event.charCode > 64 && event.charCode < 91) || (event.charCode > 96 && event.charCode < 123)' @*Added by sujata*@>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label">@PIVF.LanguageResource.Resources.lblMiddleName @*Middle Name*@</label>
                                    <div class="col-md-12 col-sm-12 col-lg-12">
                                        <input type="text" class="form-control Capitalize" name="txtMiddleName" ng-model="Patient.MiddleName" maxlength="20" multi-validation="AlphaArrowBkSpDelSp" tabindex="8"  onkeypress='return (event.charCode > 64 && event.charCode < 91) || (event.charCode > 96 && event.charCode < 123)' @*Added by sujata*@>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label"><span class="text-Red">* </span> @PIVF.LanguageResource.Resources.lblLastName @*Last Name*@</label>
                                    <div class="col-md-12 col-sm-12 col-lg-12" ng-class="{'Invalid':frmRegistration.txtLastName.$invalid && frmRegistration.txtLastName.$dirty }">
                                        <input type="text" class="form-control Capitalize" name="txtLastName" ng-model="Patient.LastName" maxlength="20" multi-validation="AlphaArrowBkSpDelSp" ng-required="true" tabindex="9"  onkeypress='return (event.charCode > 64 && event.charCode < 91) || (event.charCode > 96 && event.charCode < 123)' @*Added by sujata*@>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 col-lg-12">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label"><span class="text-Red">* </span> @PIVF.LanguageResource.Resources.lblGender @*Gender*@</label>
                                    <div class="col-md-12 col-sm-12 col-lg-12">
                                        @*ng-init="Patient.GenderID=2"*@
                                        <label class="radio-inline" id="lblPatMaleGdr">
                                            <input type="radio" ng-model="Patient.GenderID" name="rdbGender" id="rdbGender1" ng-disabled="RegType==1" ng-change="RegType==3?(Patient.GenderID==1?Partner.GenderID=2:Partner.GenderID=1):0" ng-value="1" tabindex="10">@PIVF.LanguageResource.Resources.lblMale @*Male*@
                                        </label>
                                        <label class="radio-inline" id="lblPatFemaleGdr">
                                            <input type="radio" ng-model="Patient.GenderID" name="rdbGender" id="rdbGender2" ng-disabled="RegType==1" ng-change="RegType==3?(Patient.GenderID==2?Partner.GenderID=1:Partner.GenderID=2):0" ng-value="2" tabindex="11">@PIVF.LanguageResource.Resources.lblFemale @*Female*@
                                        </label>
                                        <label class="radio-inline" id="lblPatTransGdr">
                                            <input type="radio" ng-model="Patient.GenderID" name="rdbGender" id="rdbGender3" ng-disabled="RegType==1" ng-value="3" tabindex="12">@PIVF.LanguageResource.Resources.lblTransgender @*Transgender*@
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 col-lg-6">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label">@PIVF.LanguageResource.Resources.lblDOB @*Date Of Birth*@</label><!--ng-attrs-date='{"style": style}' ng-attrs-month ='{"style": style}' ng-attrs-year ='{"style": style}'-->
                                    <div class="col-md-12 col-sm-12 col-lg-12">
                                        <ng-combo-date-picker ng-model="Patient.DateOfBirth" name="patientDOB" ng-required="Patient.Age==undefined" ng-min-model="min" ng-order="ymd" ng-max-model="max" ng-year-order="desc" ng-placeholder="Year,Month,Date" tabindex="13"></ng-combo-date-picker>
                                        <button class="btn_view p-l-5" wz-next type="button" id="" ng-click="clearDob('patient')" title="Reset"><span class="glyphicon glyphicon-refresh"></span></button>
                                    </div>

                                </div>
                            </div>
                            <div class="col-md-6 col-lg-offset-2 col-lg-4">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label"><span class="text-Red">* </span> @PIVF.LanguageResource.Resources.lblAge @*Age*@</label>
                                    <div class="col-md-12 col-sm-12 col-lg-12" ng-class="{'Invalid':frmRegistration.txtAge.$invalid && frmRegistration.txtAge.$dirty }">
                                        <input id="txtAge" name="txtAge" type="text" ng-model="Patient.Age"  class="form-control input-sm alignRight" multi-validation="NumericArrowBkSpDelSpDCFSlash" ng-required="true" maxlength="2" tabindex="14">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label"><span class="text-Red">* </span> @PIVF.LanguageResource.Resources.lblMobNo @*Mobile No.*@</label>
                                    <div class="col-md-3 col-sm-3 p-r-0" ng-class="{'Invalid':frmRegistration.txtMobCountryCode.$invalid && frmRegistration.txtMobCountryCode.$dirty}">
                                        <input type="text" class="form-control alignRight" name="txtMobCountryCode" ng-model="country" uib-typeahead="i as (i.CountryCode+'('+i.CountryName+')') for i in CountryList | filter:$viewValue | limitTo:8" typeahead-on-select="SelectedCountry(country,1)" ng-required="true" maxlength="7" tabindex="15">
                                    </div>
                                    <div class="col-md-9 col-sm-9 p-l-4" ng-class="{'Invalid':frmRegistration.txtMobNo.$invalid && frmRegistration.txtMobNo.$dirty}">
                                        <input type="text" class="form-control alignRight" name="txtMobNo" ng-model="Patient.MobileNo" ng-required="true" multi-validation="MobNoStartWith" maxlength="10" tabindex="16">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label"> @PIVF.LanguageResource.Resources.lblIdentityProof @*Identity Proof Type*@</label>                 @*<span class="text-Red">* </span>*@
                                    <div class="col-md-12 col-sm-12 col-lg-12">
                                        <select class="form-control" 
                                                name="ddlIDProofType" 
                                                ng-model="Patient.IdentityID" 
                                                ng-options="i.ID as i.Description for i in IdPrppfTypeList" 
                                                ng-change="validateIDProof(Patient.IdentityID)" 
                                                tabindex="17"
                                                ng-class="{select:Patient.IdentityID== 0 && frmRegistration.ddlIDProofType.$dirty}"></select> @*ng-required="true"*@ 
                                                                  
                                    </div>

                                </div>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label"> @PIVF.LanguageResource.Resources.lblIdProffNo @*Identity No.*@</label>         @*<span class="text-Red">* </span>*@
                                    <div class="col-md-12 col-sm-12 col-lg-12" ng-class="{'Invalid':frmRegistration.txtIdProofNo.$invalid && frmRegistration.txtIdProofNo.$dirty}">
                                        <input type="text" class="form-control" id="txtIdProofNo" name="txtIdProofNo" ng-model="Patient.IdentityNumber" multi-validation="{{Multivalidation}}" ng-change="Patient.IdentityNumber=Patient.IdentityNumber.toUpperCase();" tabindex="18"> @*ng-required="true"*@
                                    </div>
                                </div>
                            </div>
                            <div class="clear"></div>
                            <div class="col-md-6 col-lg-4">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label">@PIVF.LanguageResource.Resources.lblAttachIDProof  @*Attach Id Proof*@</label>
                                    <div class="col-md-12 col-sm-12 col-lg-12 img-attach">
                                        <button type="button" class="btn btn-primary btn-sm" data-toggle="modal" data-target="#modAttachment" ng-click="fromGallery=false;fromWebCam=false;addFor ='patient';isAttach=true" tabindex="19">@PIVF.LanguageResource.Resources.btnLink @*Link*@</button>
                                        <button class="btn_view p-l-5" ng-click="content=Patient.strAttachment;FileName=Patient.attFileName;From='IdProof';extn=Patient.attFileName.split('.').pop().toLowerCase();"><span class="glyphicon glyphicon-eye-open" data-toggle="modal" data-target="#modViewPhoto"></span></button>
                                    </div>
                                </div>
                            </div>



                          @*<div class="col-md-6 col-lg-4">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label">@PIVF.LanguageResource.Resources.lblUploadPhoto @*Upload Photo*@</label>
                                  @* <div class="col-md-12 col-sm-12 col-lg-12 img-attach">
                                        <button type="button" class="btn btn-primary btn-sm" data-toggle="modal" data-target="#modPhoto" ng-click="fromGallery=false;fromWebCam=false;addFor ='patient';isAttach=false" tabindex="20">@PIVF.LanguageResource.Resources.btnLink @*Link*@</button>
                                      @*<input type="file" id="fileInput" name="fileInput" class="upload" onchange="angular.element(this).scope().handleFileSelect(this,'patient')" />*@
                                    @* <button class="btn_view  p-l-5" ng-click="Image=Patient.strPhoto;FileName=Patient.FileName;From='Photo';extn=Patient.FileName.split('.').pop().toLowerCase();">
                                      <span class="glyphicon glyphicon-eye-open" data-toggle="modal" data-target="#modViewPhoto"></span></button>
                                    </div>
                                </div>
                            </div>*@

                 <div class="clear"></div> 
                   
                  </div>
                    </div>
                    <div class="col-lg-6 col-md-6" ng-hide="addInfo || refInfo || RegType==2 ||  RegType==3 ||  RegType==4">  @*||  RegType==3 ||  RegType==4"*@
                        <div class="row form_Content">
                            <div class="col-lg-12">
                                <h5 class="subTitle">@PIVF.LanguageResource.Resources.lblPartnerInformation @*Partner Information*@</h5>
                            </div>

                            <div class="col-lg-4 col-lg-offset-4 col-md-12 text-center">
                                <div class="patient_pic">
                                    <button type="button" class="glyphicon glyphicon-trash imgEdit" style="top:10px;right:-8px;" title="Delete Profile Image" data-toggle="modal" data-target="#modPhotoDelete" ng-disabled="(Partner.strPhoto)==null" ng-click="fromGallery=false;fromWebCam=false;addFor ='partner';isAttach=false"></button>
                                    <button type="button" class="glyphicon glyphicon-pencil imgEdit" title="Edit Profile Image" data-toggle="modal" data-target="#modPhoto" ng-click="fromGallery=false;fromWebCam=false;addFor ='partner';isAttach=false"></button>
                                    <img src="{{Partner.strPhoto}}" class="profile" alt="">
                                </div>
                            </div><div class="clear"></div>

                            <div class="col-md-6 col-lg-4">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label"><span class="text-Red">*</span>@PIVF.LanguageResource.Resources.lblFirstName @*First Name*@</label>
                                    <div class="col-md-12 col-sm-12 col-lg-12" ng-class="{'Invalid':frmRegistration.txtPartnrFirstName.$invalid && frmRegistration.txtPartnrFirstName.$dirty }">
                                        <input type="text" class="form-control Capitalize" name="txtPartnrFirstName" ng-model="Partner.FirstName" maxlength="20" multi-validation="AlphaArrowBkSpDelSp" ng-required="RegType == 1" tabindex="21"  onkeypress='return (event.charCode > 64 && event.charCode < 91) || (event.charCode > 96 && event.charCode < 123)' @*Added by sujata*@>          @*RegType!=2 || RegType!=3 || RegType!=4*@
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label">@PIVF.LanguageResource.Resources.lblMiddleName @*Middle Name*@</label>
                                    <div class="col-md-12 col-sm-12 col-lg-12">
                                        <input type="text" class="form-control Capitalize" name="txtpartnrMiddleName" ng-model="Partner.MiddleName" maxlength="20" multi-validation="AlphaArrowBkSpDelSp" tabindex="22"  onkeypress='return (event.charCode > 64 && event.charCode < 91) || (event.charCode > 96 && event.charCode < 123)' @*Added by sujata*@>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-4">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label"><span class="text-Red">* </span> @PIVF.LanguageResource.Resources.lblLastName @*Last Name*@</label>
                                    <div class="col-md-12 col-sm-12 col-lg-12" ng-class="{'Invalid':frmRegistration.txtpartnrLastName.$invalid && frmRegistration.txtpartnrLastName.$dirty }">
                                        <input type="text" class="form-control Capitalize" name="txtpartnrLastName" ng-model="Partner.LastName" maxlength="20" multi-validation="AlphaArrowBkSpDelSp" ng-required="RegType == 1" tabindex="23"  onkeypress='return (event.charCode > 64 && event.charCode < 91) || (event.charCode > 96 && event.charCode < 123)' @*Added by sujata*@>                           @*RegType!=2 || RegType!=3 || RegType!=4"*@
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 col-lg-12">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label"><span class="text-Red">* </span> @PIVF.LanguageResource.Resources.lblGender @*Gender*@</label>
                                    <div class="col-md-12 col-sm-12 col-lg-12" ng-init="Partner.GenderID=1">
                                        <label class="radio-inline" id="lblPartnrMaleGdr">
                                            <input type="radio" name="rdbPartnrGender" id="rdbPartnrGender1" ng-model="Partner.GenderID" ng-value="1" ng-disabled="RegType==1" tabindex="24">@PIVF.LanguageResource.Resources.lblMale @*Male*@
                                        </label>
                                        <label class="radio-inline" id="lblPartnrFemaleGdr">
                                            <input type="radio" name="inlineRadioOptions" id="rdbPartnrGender2" ng-model="Partner.GenderID" ng-value="2" ng-disabled="RegType==1" tabindex="25">@PIVF.LanguageResource.Resources.lblFemale @*Female*@
                                        </label>
                                        <label class="radio-inline" id="lblPartnrTransGdr">
                                            <input type="radio" name="inlineRadioOptions" id="rdbPartnrGender3" ng-model="Partner.GenderID" ng-value="3" ng-disabled="RegType==1" tabindex="26">@PIVF.LanguageResource.Resources.lblTransgender @*Transgender*@
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-12 col-lg-6">
                                <div class="form-group ">
                                    <label class="col-md-12 col-sm-12 col-lg-12 control-label">@PIVF.LanguageResource.Resources.lblDOB @*Date Of Birth*@</label>
                                    <div class="col-md-12 col-sm-12 col-lg-12" ng-class="{'Invalid':!frmRegistration.partnrDOB.$valid && frmRegistration.partnrDOB.$dirty}">
                                        <ng-combo-date-picker ng-model="Partner.DateOfBirth" name="partnrDOB" ng-min-model="min" ng-order="ymd" ng-max-model="max" ng-year-order="desc" ng-placeholder="Year,Month,Date" tabindex="27"></ng-combo-date-picker>           @*Partner.Age==undefined &&  (RegType!=2 || RegType!=3 || RegType!=4)*@
                                        <button class="btn_view p-l-5" wz-next type="button" id="" ng-click="clearDob('partner')" title="Reset"><span class="glyphicon glyphicon-refresh"></span></button>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-offset-0 col-md-6 col-lg-offset-2 col-lg-4">
                                <div class="form-group ">
