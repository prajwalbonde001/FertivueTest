    <div class="modal fade" tabindex="-1" role="dialog" id="modPhoto" data-keyboard="false" data-backdrop="static">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">@PIVF.LanguageResource.Resources.btnUploadPhoto </h4>
                </div>
                <div class="modal-body">
                    <div>
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="text-center option-btn">
                                    <button class="btn btn-success gallary-btn " type="button" id="" ng-click="fromGallery=true;fromWebCam=false"><span class="glyphicon glyphicon-picture"></span>@PIVF.LanguageResource.Resources.btnChooseFromGallary</button>
                                    <button class="btn btn-warning Camera-btn" type="button" id="" ng-click="fromWebCam=true;fromGallery=false"><span class=""><i class="fa fa-camera"></i></span>@PIVF.LanguageResource.Resources.btnChooseFromCamera</button>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="GallaryWrapper" ng-show="fromGallery">
                                    <div class="row">
                                        <div class="col-sm-12 text-center">
                                            <h4 class="ModalHeading">@PIVF.LanguageResource.Resources.lblImgFromGallery</h4>
                                        </div>
                                        <div class="col-sm-offset-3 col-sm-6 croparea">
                                            <ui-cropper image="myImage" live-view="blockingObject" result-image="myCroppedImage" result-image-format="image/jpeg">
                                            </ui-cropper>
                                        </div>
                                        <div class="col-sm-offset-3 col-sm-6">
                                            <div class="form-group text-center" style="padding-left: 60px;">
                                                <div class="fileUpload btn btn-primary">
                                                    <span><span class="glyphicon glyphicon-paperclip"></span>@PIVF.LanguageResource.Resources.lblUploadImage</span>
                                                    <input type="file" id="fileInput" name="fileInput" class="upload" onchange="angular.element(this).scope().handleFileSelect(this)" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="PhotoCaptureWrapper" ng-show="fromWebCam">
                                    <div class="row" ng-init="webcamError=false">
                                        <div class="col-sm-12 text-center">
                                            <h4 class="ModalHeading">@PIVF.LanguageResource.Resources.lblCaptImg </h4>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-offset-3 col-sm-6" ng-show="webcamError" style="color:red">
                                                <span>@PIVF.LanguageResource.Resources.msgWebCamError @*Webcam could not be started. Did you give access to it?*@</span>
                                            </div>
                                            <webcam class="col-sm-offset-3 col-sm-6 cropArea customCropArea GallaryWrapper" channel="channel"
                                                    on-streaming="onSuccess()"
                                                    on-error="onError(err)"
                                                    on-stream="onStream(stream)">
                                            </webcam>
                                            <canvas ng-hide="true" id="snapshot" width="300" height="300"></canvas>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-offset-3 col-sm-6">
                                                <div class="form-group text-center">
                                                    <button type="button" class="btn btn-primary  m-t-10" ng-click="makeSnapshot();addFor = 'partner'"><span><i class="fa fa-camera-retro"></i></span>@PIVF.LanguageResource.Resources.btnCapture</button>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-offset-3 col-sm-6 croparea  m-b-10">
                                                <ui-cropper image="myImage" live-view="blockingObject" result-image="myCroppedImage" result-image-format="image/jpeg">
                                                </ui-cropper>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" ng-click="captureImage()">@PIVF.LanguageResource.Resources.btnSave @*Save*@</button>
                    <button type="button" class="btn btn-default" ng-click="closePhotoCapture()">@PIVF.LanguageResource.Resources.btnClose @*Close*@</button>
                </div>
            </div>
        </div>
    </div>

